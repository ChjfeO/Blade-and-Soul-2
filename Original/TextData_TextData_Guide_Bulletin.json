{
    "42000000": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 별미 중 별미",
        "id": 42000000,
        "alias": "Contents.Quest.Guide.2000000",
        "text": "(이게 별미 중에서도\n별미란 말이지?)"
    },
    "42000001": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 도치도치 대소동",
        "id": 42000001,
        "alias": "Contents.Quest.Guide.2000001",
        "text": "(제법 쫓아냈으니 \n한동안은 괜찮을 거야)"
    },
    "42000002": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 도치가 숨겨둔 반짝이!",
        "id": 42000002,
        "alias": "Contents.Quest.Guide.2000002",
        "text": "(<textColor=[255:204:102:255]>반짝이</>를 찾았다!\n<textColor=[78:148:215:255]>바롱</>이 좋아하겠군)"
    },
    "42000003": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 흔들리는 연잎 속에서…",
        "id": 42000003,
        "alias": "Contents.Quest.Guide.2000003",
        "text": "(<textColor=[255:204:102:255]>연잎 조각</>으로 \n무엇을 하려는 걸까?)"
    },
    "42000004": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 움직이는 연잎",
        "id": 42000004,
        "alias": "Contents.Quest.Guide.2000004",
        "text": "(정말 기척도 없이 \n불쑥 튀어나오는군. \n놀랄 만도 하겠어)"
    },
    "42000005": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 우산을 만들 수 있어",
        "id": 42000005,
        "alias": "Contents.Quest.Guide.2000005",
        "text": "(이 정도 크기면 비를 \n막고도 충분하겠는데?)"
    },
    "42000006": {
        "deprecated": false,
        "Dev_Desc": "[LV.6] 시궁창의 주인들",
        "id": 42000006,
        "alias": "Contents.Quest.Guide.2000006",
        "text": "(이 <textColor=[110:216:175:255]>벌목지</>는 <textColor=[215:94:94:255]>서인족</>이\n점령해버렸군. 잡아도\n잡아도 끝이 없어)"
    },
    "42000007": {
        "deprecated": false,
        "Dev_Desc": "[LV.6] 냄새의 원인",
        "id": 42000007,
        "alias": "Contents.Quest.Guide.2000007",
        "text": "(정말 지독한 냄새야.\n적응이 안되는군)"
    },
    "42000008": {
        "deprecated": false,
        "Dev_Desc": "[LV.6] 서인족의 무기 연구",
        "id": 42000008,
        "alias": "Contents.Quest.Guide.2000008",
        "text": "(흐음... <textColor=[215:94:94:255]>서인족</>의 무기라.\n딱히 쓸만해 보이진 않아)"
    },
    "42000009": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 단단한 껍질은 유용해",
        "id": 42000009,
        "alias": "Contents.Quest.Guide.2000009",
        "text": "(<textColor=[215:94:94:255]>호수 거북</>의 껍질은\n단단해서 여러모로\n활용하기가 좋겠어)"
    },
    "42000010": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 거북이는 죄가 없다냥!",
        "id": 42000010,
        "alias": "Contents.Quest.Guide.2000010",
        "text": "(<textColor=[255:204:102:255]>쓰레기 더미</>로군. \n이런 걸 누가 버린 거야?)"
    },
    "42000011": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 거북이는 죄가 있어",
        "id": 42000011,
        "alias": "Contents.Quest.Guide.2000011",
        "text": "(이 정도면 충분하겠군)"
    },
    "42000012": {
        "deprecated": false,
        "Dev_Desc": "[LV.8] 유적지를 지켜달라냥!",
        "id": 42000012,
        "alias": "Contents.Quest.Guide.2000012",
        "text": "(이 정도 시간이면 \n조사를 끝마쳤을 거야)"
    },
    "42000013": {
        "deprecated": false,
        "Dev_Desc": "[LV.8] 움직이는 바위다냥!",
        "id": 42000013,
        "alias": "Contents.Quest.Guide.2000013",
        "text": "(이 정도면 조사하기 \n충분한 시간이었을 거야)"
    },
    "42000014": {
        "deprecated": false,
        "Dev_Desc": "[LV.8] 신비한 부스러기",
        "id": 42000014,
        "alias": "Contents.Quest.Guide.2000014",
        "text": "(이거로군? \n언뜻 보기엔 평범한 \n돌멩이 같은데...?)"
    },
    "42000015": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 목초지의 골칫거리",
        "id": 42000015,
        "alias": "Contents.Quest.Guide.2000015",
        "text": "(이 정도면 충분하겠지? \n양들이 민감하다 보니 \n목장주가 고생이구나)"
    },
    "42000016": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 자연의 약재, 첫번째",
        "id": 42000016,
        "alias": "Contents.Quest.Guide.2000016",
        "text": "(묘한 냄새가 나는데...\n<textColor=[255:204:102:255]>약재</>로 사용해도\n괜찮은 걸까?)"
    },
    "42000017": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 자연의 약재, 두번째",
        "id": 42000017,
        "alias": "Contents.Quest.Guide.2000017",
        "text": "(이게 바로 <textColor=[78:148:215:255]>수양</>에서\n자주 사용하는 <textColor=[255:204:102:255]>약초</>로군)"
    },
    "42000018": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 붉은 염료가 필요해",
        "id": 42000018,
        "alias": "Contents.Quest.Guide.2000018",
        "text": "(붉은빛이 정말 곱군. \n색을 내기 좋아 보여)"
    },
    "42000019": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 황금 염료가 필요해",
        "id": 42000019,
        "alias": "Contents.Quest.Guide.2000019",
        "text": "(화려한 데다가 \n희귀하기까지 하니 \n인기가 많겠어)"
    },
    "42000020": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 잃어버린 비법서",
        "id": 42000020,
        "alias": "Contents.Quest.Guide.2000020",
        "text": "(여기 있다! \n<textColor=[255:204:102:255]>바롱의 비법서</>라니, \n내용이 궁금한걸...)"
    },
    "42000021": {
        "deprecated": false,
        "Dev_Desc": "[LV.16] 피의 복수",
        "id": 42000021,
        "alias": "Contents.Quest.Guide.2000021",
        "text": "(후~ 힘들었다.\n<textColor=[215:94:94:255]>창도단</>의 세가 날이 \n갈수록 강해지고 있어)"
    },
    "42000022": {
        "deprecated": false,
        "Dev_Desc": "[LV.16] 창도단의 독특한 무기",
        "id": 42000022,
        "alias": "Contents.Quest.Guide.2000022",
        "text": "(여기 있군! \n이 정도면 연구하기 \n충분할 거야)"
    },
    "42000023": {
        "deprecated": false,
        "Dev_Desc": "[LV.16] 의외의 유행품",
        "id": 42000023,
        "alias": "Contents.Quest.Guide.2000023",
        "text": "(...이게 왜 유행이지? \n알다가도 모를 일이야)"
    },
    "42000024": {
        "deprecated": false,
        "Dev_Desc": "[LV.16] 도적단을 소탕하라",
        "id": 42000024,
        "alias": "Contents.Quest.Guide.2000024",
        "text": "(후~ 이 정도면 됐겠지? \n<textColor=[215:94:94:255]>창도단</> 때문에 <textColor=[78:148:215:255]>대어방</>이 \n골치가 아프겠어)"
    },
    "42000025": {
        "deprecated": false,
        "Dev_Desc": "[LV.15] 행인 보호 임무",
        "id": 42000025,
        "alias": "Contents.Quest.Guide.2000025",
        "text": "(후~ 힘들었다.\n당분간은 안전하게\n산책할 수 있을 거야)"
    },
    "42000026": {
        "deprecated": false,
        "Dev_Desc": "[LV.15] 흘리고 온 봇짐",
        "id": 42000026,
        "alias": "Contents.Quest.Guide.2000026",
        "text": "(찾았다! <textColor=[110:216:175:255]>고도시 숲길</>은\n일반인이 지나기에\n너무 위험해 보여)"
    },
    "42000027": {
        "deprecated": false,
        "Dev_Desc": "[LV.15] 제대로 된 약재",
        "id": 42000027,
        "alias": "Contents.Quest.Guide.2000027",
        "text": "(정말 <textColor=[255:204:102:255]>단단한 뿔</>이로군. \n이 정도면 <textColor=[255:204:102:255]>약재</>로 \n만들기 충분하겠어)"
    },
    "42000028": {
        "deprecated": false,
        "Dev_Desc": "[LV.15] 정령, 가지, 없다...",
        "id": 42000028,
        "alias": "Contents.Quest.Guide.2000028",
        "text": "(...왠지 <textColor=[78:148:215:255]>바롱</>이라면 \n<textColor=[215:94:94:255]>수목 정령</>을 해치지 않고 \n가지를 얻었을 거 같군)"
    },
    "42000029": {
        "deprecated": false,
        "Dev_Desc": "[LV.14] 투엽단을 저지하라",
        "id": 42000029,
        "alias": "Contents.Quest.Guide.2000029",
        "text": "(<textColor=[78:148:215:255]>잿빛바람</>의 마음이\n조금이나마 \n편안해졌으면 좋겠어)"
    },
    "42000030": {
        "deprecated": false,
        "Dev_Desc": "[LV.14] 동물들의 안식을 위해",
        "id": 42000030,
        "alias": "Contents.Quest.Guide.2000030",
        "text": "(...<textColor=[215:94:94:255]>투엽단</>의 밀렵에\n희생당한 동물들이 \n부디 편안하게 잠들기를...)"
    },
    "42000031": {
        "deprecated": false,
        "Dev_Desc": "[LV.30] 불마의 존재(1)",
        "id": 42000031,
        "alias": "Contents.Quest.Guide.2000031",
        "text": "(<textColor=[215:94:94:255]>불마자</>가 균열에서\n끝도 없이 쏟아져\n나오는구나...)"
    },
    "42000032": {
        "deprecated": false,
        "Dev_Desc": "[LV.30] 강력한 적",
        "id": 42000032,
        "alias": "Contents.Quest.Guide.2000032",
        "text": "(정말 강하군. \n<textColor=[78:148:215:255]>에레다</>가 아니었다면\n피해가 더 컸을 거야)"
    },
    "42000033": {
        "deprecated": false,
        "Dev_Desc": "[LV.30] 균열의 흔적",
        "id": 42000033,
        "alias": "Contents.Quest.Guide.2000033",
        "text": "(충분히 모았다.\n<textColor=[255:204:102:255]>균열의 흔적</>을 연구하여\n대책을 찾을 수 있기를...)"
    },
    "42000034": {
        "deprecated": false,
        "Dev_Desc": "[LV.23] 맹수들의 위협",
        "id": 42000034,
        "alias": "Contents.Quest.Guide.2000034",
        "text": "(모두 처치했다. \n이 <textColor=[110:216:175:255]>구릉지</>의 야생동물은 \n정말 난폭한 것 같아)"
    },
    "42000035": {
        "deprecated": false,
        "Dev_Desc": "[LV.23] 덧가죽을 구해줘",
        "id": 42000035,
        "alias": "Contents.Quest.Guide.2000035",
        "text": "(온전한 <textColor=[255:204:102:255]>동물 가죽</>을\n충분히 구한 것 같군)"
    },
    "42000036": {
        "deprecated": false,
        "Dev_Desc": "[LV.23] 잃어버린 짐",
        "id": 42000036,
        "alias": "Contents.Quest.Guide.2000036",
        "text": "(이게 <textColor=[78:148:215:255]>상인</>이 떨어트린\n<textColor=[255:204:102:255]>봇짐</>이로구나)"
    },
    "42000037": {
        "deprecated": false,
        "Dev_Desc": "[LV.18] 소중한 인형",
        "id": 42000037,
        "alias": "Contents.Quest.Guide.2000037",
        "text": "(<textColor=[215:94:94:255]>혼령</>이 아이의\n<textColor=[255:204:102:255]>인형</>을 가져가다니.\n이상한 일이야)"
    },
    "42000038": {
        "deprecated": false,
        "Dev_Desc": "[LV.18] 원혼을 달래야한다냥",
        "id": 42000038,
        "alias": "Contents.Quest.Guide.2000038",
        "text": "(...정말 이 방법으로 \n<textColor=[215:94:94:255]>령</>을 달래고 \n성불시킬 수 있을까...?)"
    },
    "42000039": {
        "deprecated": false,
        "Dev_Desc": "[LV.18] 나, 김서방, 아니다.",
        "id": 42000039,
        "alias": "Contents.Quest.Guide.2000039",
        "text": "(휴, 끝마쳤다.\n<textColor=[215:94:94:255]>도깨비</>의 의뢰라니,\n이상한 경험이었어)"
    },
    "42000040": {
        "deprecated": false,
        "Dev_Desc": "[LV.26] 조금 다른 생물들",
        "id": 42000040,
        "alias": "Contents.Quest.Guide.2000040",
        "text": "(이 정도면 충분해.\n균열에 대한 실마리를 \n찾을 수 있으면 좋을텐데)"
    },
    "42000041": {
        "deprecated": false,
        "Dev_Desc": "[LV.26] 두고 온 물건",
        "id": 42000041,
        "alias": "Contents.Quest.Guide.2000041",
        "text": "(...찾았다! <textColor=[78:148:215:255]>수와족</>은\n짐을 챙길 여력도 없이\n바쁘게 떠났나 보다)"
    },
    "42000042": {
        "deprecated": false,
        "Dev_Desc": "[LV.26] 수와족 마을 재건 사업",
        "id": 42000042,
        "alias": "Contents.Quest.Guide.2000042",
        "text": "(의뢰를 완료했지만... \n아직도 마을 안은\n<textColor=[215:94:94:255]>침식된 괴물</>로 가득하다)"
    },
    "42000043": {
        "deprecated": false,
        "Dev_Desc": "[LV.26] 불마의 존재(2)",
        "id": 42000043,
        "alias": "Contents.Quest.Guide.2000043",
        "text": "(후, 끝마쳤다.\n<textColor=[78:148:215:255]>수와족</>에게 작은 \n도움이라도 되었기를)"
    },
    "42000044": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 수양 협행 전문가",
        "id": 42000044,
        "alias": "Contents.Quest.Guide.2000044",
        "text": "(<textColor=[78:148:215:255]>수양</>의 협행을 완료했다. \n대부분 탐사에 필요한\n자원을 수집하는 의뢰였네)"
    },
    "42000045": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 에레다 협행 전문가",
        "id": 42000045,
        "alias": "Contents.Quest.Guide.2000045",
        "text": "(<textColor=[78:148:215:255]>에레다</>가 <textColor=[215:94:94:255]>불마자</> 처치에\n힘쓴 덕분에 주민들이\n조금 더 안전해졌어)"
    },
    "42000046": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 도화상전 협행 전문가",
        "id": 42000046,
        "alias": "Contents.Quest.Guide.2000046",
        "text": "(<textColor=[78:148:215:255]>도화상전</>의 협행을 \n모두 완료했어.\n보상이 기대되는걸?)"
    },
    "42000047": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 영교원 협행 전문가",
        "id": 42000047,
        "alias": "Contents.Quest.Guide.2000047",
        "text": "(<textColor=[78:148:215:255]>영교원</>의 협행은 어딘가\n다정하면서도 신기하네.\n완료 보상을 받아볼까?)"
    },
    "42000048": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 금경철문 협행 전문가",
        "id": 42000048,
        "alias": "Contents.Quest.Guide.2000048",
        "text": "(휴, 힘들었다. <textColor=[78:148:215:255]>금경철문</>이\n더 좋은 무기를 만드는데\n도움이 되었다면 좋겠어)"
    },
    "42000049": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 대어방 협행 전문가",
        "id": 42000049,
        "alias": "Contents.Quest.Guide.2000049",
        "text": "(<textColor=[78:148:215:255]>대어방 주민</>들의\n협행을 충분히 완료했다)"
    },
    "42000050": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 비사문 협행 전문가",
        "id": 42000050,
        "alias": "Contents.Quest.Guide.2000050",
        "text": "(<textColor=[78:148:215:255]>반달호수의 주민</>들에게\n조금이나마 도움이 되었기를)"
    },
    "42000051": {
        "deprecated": false,
        "Dev_Desc": "[LV.7] 도치가 숨겨둔 반짝이!",
        "id": 42000051,
        "alias": "Contents.Quest.Guide.2000051",
        "text": "(저기 뭔가 반짝인다!)"
    },
    "42000052": {
        "deprecated": false,
        "Dev_Desc": "[LV.6] 서인족의 무기 연구",
        "id": 42000052,
        "alias": "Contents.Quest.Guide.2000052",
        "text": "(어디 보자... 여기 어딘가 \n<textColor=[215:94:94:255]>서인족</>이 무기를 모아둔\n상자가 있을 것 같은데...)"
    },
    "42000053": {
        "deprecated": false,
        "Dev_Desc": "[LV.8] 신비한 부스러기",
        "id": 42000053,
        "alias": "Contents.Quest.Guide.2000053",
        "text": "(주변의 위협은\n충분히 처리했어)"
    },
    "42000054": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 자연의 약재, 두번째",
        "id": 42000054,
        "alias": "Contents.Quest.Guide.2000054",
        "text": "(좋아! 이 정도면 \n<textColor=[255:204:102:255]>약초</>를 채집할 수 \n있을 것 같아)"
    },
    "42000055": {
        "deprecated": false,
        "Dev_Desc": "[LV.21] 잃어버린 비법서",
        "id": 42000055,
        "alias": "Contents.Quest.Guide.2000055",
        "text": "(주변의 위협은\n충분히 제거했다)"
    },
    "42000056": {
        "deprecated": false,
        "Dev_Desc": "[LV.16] 창도단의 독특한 무기",
        "id": 42000056,
        "alias": "Contents.Quest.Guide.2000056",
        "text": "(어디 보자. 어딘가 \n<textColor=[255:204:102:255]>무기 상자</>가 있을 텐데...)"
    },
    "42000057": {
        "deprecated": false,
        "Dev_Desc": "[LV.23] 잃어버린 짐",
        "id": 42000057,
        "alias": "Contents.Quest.Guide.2000057",
        "text": "(이제 <textColor=[255:204:102:255]>봇짐</>을 회수하자)"
    },
    "42000058": {
        "deprecated": false,
        "Dev_Desc": "[LV.15] 흘리고 온 봇짐",
        "id": 42000058,
        "alias": "Contents.Quest.Guide.2000058",
        "text": "(이정도면 충분해.\n이제 <textColor=[255:204:102:255]>봇짐</>을 회수하자)"
    },
    "42000059": {
        "deprecated": false,
        "Dev_Desc": "[LV.14] 동물들의 안식을 위해",
        "id": 42000059,
        "alias": "Contents.Quest.Guide.2000059",
        "text": "(방해가 되는 <textColor=[215:94:94:255]>투엽단</>은 \n충분히 처치했다)"
    },
    "42000060": {
        "deprecated": false,
        "Dev_Desc": "[LV.26] 두고 온 물건",
        "id": 42000060,
        "alias": "Contents.Quest.Guide.2000060",
        "text": "(짐 주변의 위협은\n충분히 제거했다.\n<textColor=[255:204:102:255]>수와의 짐</>을 수거하자)"
    },
    "42000061": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 썩지 않는 난파선",
        "id": 42000061,
        "alias": "Contents.Quest.Guide.2000061",
        "text": "(휴~ 힘들었다. 그나저나, \n<textColor=[255:204:102:255]>난파선</>에 타고 있던 \n사람들은 어떻게 됐을까?)"
    },
    "42000062": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 소라게의 독",
        "id": 42000062,
        "alias": "Contents.Quest.Guide.2000062",
        "text": "(충분히 얻었어. \n독성이 있다니 함부로 \n먹으면 안 되겠군)"
    },
    "42000063": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 귀한 보양식",
        "id": 42000063,
        "alias": "Contents.Quest.Guide.2000063",
        "text": "(이 정도면 충분해. \n맛이 일품이라니,\n언제 한번 먹어봐야겠군)"
    },
    "42000064": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 떠밀려온 난파선 조사",
        "id": 42000064,
        "alias": "Contents.Quest.Guide.2000064",
        "text": "(드디어 끝마쳤다.\n<textColor=[78:148:215:255]>조사원</>들이 안전하게\n둘러볼 수 있겠어)"
    },
    "42000065": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 반달초를 구해달라냥!_1",
        "id": 42000065,
        "alias": "Contents.Quest.Guide.2000065",
        "text": "(안전해진 틈을 타\n<textColor=[255:204:102:255]>반달초</>를 채집하자!)"
    },
    "42000066": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.27] 반달초를 구해달라냥!_2",
        "id": 42000066,
        "alias": "Contents.Quest.Guide.2000066",
        "text": "(이거로군! \n좋은 향기가 나는걸?)"
    },
    "42000067": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 최고의 재료",
        "id": 42000067,
        "alias": "Contents.Quest.Guide.2000067",
        "text": "(충분히 모았다.\n튼튼해서 갑옷으로 \n만들면 딱이겠어)"
    },
    "42000068": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 진흙 속에 사는 물고기",
        "id": 42000068,
        "alias": "Contents.Quest.Guide.2000068",
        "text": "(드디어 끝마쳤다.\n<textColor=[255:204:102:255]>물고기</>가 보이진 않는데…)"
    },
    "42000069": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 악어 가죽이 필요해",
        "id": 42000069,
        "alias": "Contents.Quest.Guide.2000069",
        "text": "(모두 구했다.\n제법 까다로운 임무였어)"
    },
    "42000070": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 수양대원을 위한 특식",
        "id": 42000070,
        "alias": "Contents.Quest.Guide.2000070",
        "text": "(이 정도면 <textColor=[78:148:215:255]>수양대원</> 전부가\n배부르게 먹을 수 있겠어)"
    },
    "42000071": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 늪지에서 피는 꽃_1",
        "id": 42000071,
        "alias": "Contents.Quest.Guide.2000071",
        "text": "(휴, 조금 안전해졌네.\n어서 <textColor=[255:204:102:255]>늪지 약초</>를 채집하자)"
    },
    "42000072": {
        "deprecated": false,
        "Dev_Desc": "반달호수_[Lv.28] 늪지에서 피는 꽃_2",
        "id": 42000072,
        "alias": "Contents.Quest.Guide.2000072",
        "text": "(하얗고 예쁜 꽃이네.\n진흙 속에서 자랐다는 게\n믿기지 않을 정도야)"
    },
    "42001000": {
        "deprecated": false,
        "Dev_Desc": "[LV.30] 기름진 발발이",
        "id": 42001000,
        "alias": "Contents.Quest.Guide.2001000",
        "text": "(정말 기름지네. \n잘 말려 사용하면 \n연료로 손색이 없겠어)"
    },
    "42001001": {
        "deprecated": false,
        "Dev_Desc": "[LV.30] 거미줄 연구",
        "id": 42001001,
        "alias": "Contents.Quest.Guide.2001001",
        "text": "(<textColor=[215:94:94:255]>밀림 거미</>의 거미줄은 \n정말 굵고 튼튼하군. \n연구할 가치가 있어 보여)"
    },
    "42001002": {
        "deprecated": false,
        "Dev_Desc": "[LV.31] 가죽의 가치",
        "id": 42001002,
        "alias": "Contents.Quest.Guide.2001002",
        "text": "(이 정도면 충분해. 이젠 \n<textColor=[215:94:94:255]>곰</>과 <textColor=[215:94:94:255]>호랑이</>가 돌아오길 \n기다리는 수밖에…)"
    },
    "42001003": {
        "deprecated": false,
        "Dev_Desc": "[LV.31] 자꾸 없어지는 비법서",
        "id": 42001003,
        "alias": "Contents.Quest.Guide.2001003",
        "text": "(찾았다! 이거로군. \n약초에 정신이 쏠려 \n<textColor=[255:204:102:255]>비법서</>를 잊었나 보다)"
    },
    "42001004": {
        "deprecated": false,
        "Dev_Desc": "[LV.32] 방어선 구축",
        "id": 42001004,
        "alias": "Contents.Quest.Guide.2001004",
        "text": "(좋아. 이곳에 방어시설이 \n만들어지면 거점이 \n훨씬 안전해질 거야)"
    },
    "42001005": {
        "deprecated": false,
        "Dev_Desc": "[LV.32] 늑대 집단",
        "id": 42001005,
        "alias": "Contents.Quest.Guide.2001005",
        "text": "(휴, 끝마쳤다.\n이젠 <textColor=[78:148:215:255]>에레다</>가\n마무리할 차례야)"
    },
    "42001006": {
        "deprecated": false,
        "Dev_Desc": "[LV.32] 새로운 염료 개발",
        "id": 42001006,
        "alias": "Contents.Quest.Guide.2001006",
        "text": "(충분히 얻었다. \n<textColor=[215:94:94:255]>오똑 발발이</>의 풀은 \n짙은 연두색을 낼 것 같아)"
    },
    "42001007": {
        "deprecated": false,
        "Dev_Desc": "[LV.33] 만다라화 소음 문제",
        "id": 42001007,
        "alias": "Contents.Quest.Guide.2001007",
        "text": "(임무를 완수했다. \n수가 조금 줄긴 했지만 \n여전히 시끄럽군)"
    },
    "42001008": {
        "deprecated": false,
        "Dev_Desc": "[LV.34] 장인들의 별미!",
        "id": 42001008,
        "alias": "Contents.Quest.Guide.2001008",
        "text": "(모두 구했다. \n쫄깃쫄깃한 식감이\n일품일 것 같다)"
    },
    "42001009": {
        "deprecated": false,
        "Dev_Desc": "[LV.34] 거북 알 채집",
        "id": 42001009,
        "alias": "Contents.Quest.Guide.2001009",
        "text": "(<textColor=[255:204:102:255]>거북알</>을 구했다. \n남획으로 인해 멸종되는 \n일은 없어야 할 텐데...)"
    },
    "42001010": {
        "deprecated": false,
        "Dev_Desc": "[LV.35] 연안 거북의 껍질",
        "id": 42001010,
        "alias": "Contents.Quest.Guide.2001010",
        "text": "(<textColor=[110:216:175:255]>해월대륙</>에서도\n단단한 <textColor=[255:204:102:255]>거북의 등껍질</>은 \n유용하게 사용되는구나)"
    },
    "42001011": {
        "deprecated": false,
        "Dev_Desc": "[LV.35] 어둑시니 퇴치",
        "id": 42001011,
        "alias": "Contents.Quest.Guide.2001011",
        "text": "(모두 처치했다.\n<textColor=[78:148:215:255]>명혜단</>은 <textColor=[110:216:175:255]>난파선의 잔해</>를\n탐사할 계획일까?)"
    },
    "42001012": {
        "deprecated": false,
        "Dev_Desc": "[LV.37] 식인악어 퇴치",
        "id": 42001012,
        "alias": "Contents.Quest.Guide.2001012",
        "text": "(휴, 힘들었다. \n이곳의 <textColor=[215:94:94:255]>악어</>들은\n공격성이 정말로 강하군)"
    },
    "42001013": {
        "deprecated": false,
        "Dev_Desc": "[LV.40] 단단한 식물의 줄기",
        "id": 42001013,
        "alias": "Contents.Quest.Guide.2001013",
        "text": "(정말 질기고 단단하군. \n바구니를 만들기에\n안성맞춤이겠어)"
    },
    "42001014": {
        "deprecated": false,
        "Dev_Desc": "[LV.40] 잃어버린 짐을 찾아요",
        "id": 42001014,
        "alias": "Contents.Quest.Guide.2001014",
        "text": "(<textColor=[255:204:102:255]>봇짐</>을 찾았다. \n거점 주변이 조금 더 \n안전해지면 좋을 텐데)"
    },
    "42001015": {
        "deprecated": false,
        "Dev_Desc": "[LV.41] 혈비패 축출",
        "id": 42001015,
        "alias": "Contents.Quest.Guide.2001015",
        "text": "(임무를 끝마쳤다.\n<textColor=[215:94:94:255]>혈비패</>를 멸살하려면\n많은 인원이 필요하겠어)"
    },
    "42001016": {
        "deprecated": false,
        "Dev_Desc": "[LV.41] 혈비패 두건 태우기",
        "id": 42001016,
        "alias": "Contents.Quest.Guide.2001016",
        "text": "(모두 처치했다.\n두건을 가진 이들이\n확실히 더 강하군)"
    },
    "42001017": {
        "deprecated": false,
        "Dev_Desc": "[LV.42] 달빛바람의 명예",
        "id": 42001017,
        "alias": "Contents.Quest.Guide.2001017",
        "text": "(어두운 곳에서 보면\n정말 헷갈릴 수 있겠어)"
    },
    "42001018": {
        "deprecated": false,
        "Dev_Desc": "[LV.42] 투연초 연구",
        "id": 42001018,
        "alias": "Contents.Quest.Guide.2001018",
        "text": "(...독도 약에 쓰이니\n<textColor=[215:94:94:255]>투연초</>도 연구해 볼\n가치가 있을 거야)"
    },
    "42001019": {
        "deprecated": false,
        "Dev_Desc": "[LV.42] 곽호룡의 위신",
        "id": 42001019,
        "alias": "Contents.Quest.Guide.2001019",
        "text": "(휴, 힘들었다.\n그나저나... 정말 닮았군)"
    },
    "42001020": {
        "deprecated": false,
        "Dev_Desc": "[LV.43] 개척지로 가는 길",
        "id": 42001020,
        "alias": "Contents.Quest.Guide.2001020",
        "text": "(이 정도면 무사히 \n지나갈 수 있을 거야)"
    },
    "42001021": {
        "deprecated": false,
        "Dev_Desc": "[LV.43] 갑작스런 위험",
        "id": 42001021,
        "alias": "Contents.Quest.Guide.2001021",
        "text": "(됐다. 이 정도면 \n<textColor=[78:148:215:255]>바롱</>이 안전하게 조사를 \n진행할 수 있을 거야)"
    },
    "42001022": {
        "deprecated": false,
        "Dev_Desc": "[LV.44] 뱀버섯 해독제",
        "id": 42001022,
        "alias": "Contents.Quest.Guide.2001022",
        "text": "(저 치명적으로 보이는\n<textColor=[215:94:94:255]>뱀버섯</>의 독이 약으로\n쓰인다니... 신기하군)"
    },
    "42001023": {
        "deprecated": false,
        "Dev_Desc": "[LV.45] 묘지 근처 치안 확보",
        "id": 42001023,
        "alias": "Contents.Quest.Guide.2001023",
        "text": "(<textColor=[215:94:94:255]>영혼불</> 제거를 끝마쳤다.\n이상한 건 보지 못했는데...)"
    },
    "42001024": {
        "deprecated": false,
        "Dev_Desc": "[LV.45] 묘지의 혈비패 퇴치",
        "id": 42001024,
        "alias": "Contents.Quest.Guide.2001024",
        "text": "(드디어 끝마쳤군.\n<textColor=[215:94:94:255]>혈비패</>는 뭘 얻기 위해\n묘지에 온 걸까?)"
    },
    "42001025": {
        "deprecated": false,
        "Dev_Desc": "[LV.46] 묘약 개발",
        "id": 42001025,
        "alias": "Contents.Quest.Guide.2001025",
        "text": "(이 정도면 충분해. \n만병통치약이라니... \n만들어지면 정말 좋겠군)"
    },
    "42001026": {
        "deprecated": false,
        "Dev_Desc": "[LV.47] 좋은 가죽 얻기",
        "id": 42001026,
        "alias": "Contents.Quest.Guide.2001026",
        "text": "(낯선 형태의 무기로군. \n연구할 가치가 있어 보여)"
    },
    "42001027": {
        "deprecated": false,
        "Dev_Desc": "[LV.47] 담인초 새로운 연구",
        "id": 42001027,
        "alias": "Contents.Quest.Guide.2001027",
        "text": "(이 정도면 충분하겠지? \n<textColor=[215:94:94:255]>담인초</>는 봐도 봐도 \n적응이 되지 않는군)"
    },
    "42001028": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 습격자 낙원족",
        "id": 42001028,
        "alias": "Contents.Quest.Guide.2001028",
        "text": "(휴, 겨우 끝마쳤다. \n<textColor=[215:94:94:255]>낙원족</>은 공격성이 강해서 \n상대하기가 만만치 않군)"
    },
    "42001029": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 연잎으로 우산 제작",
        "id": 42001029,
        "alias": "Contents.Quest.Guide.2001029",
        "text": "(모두 구했다!\n<textColor=[110:216:175:255]>반달 호수</>의 연잎만큼이나 \n크고 단단하군)"
    },
    "42001030": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 언덕길 관리",
        "id": 42001030,
        "alias": "Contents.Quest.Guide.2001030",
        "text": "(<textColor=[78:148:215:255]>명혜단</>의 바람처럼\n이곳이 오래 보존되기를)"
    },
    "42001031": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 사묘정 몰아내기",
        "id": 42001031,
        "alias": "Contents.Quest.Guide.2001031",
        "text": "(휴, 임무를 완수했다. \n언뜻 보면 수풀처럼 보여 \n더 위험한 것 같아)"
    },
    "42001032": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 삼색조 기름",
        "id": 42001032,
        "alias": "Contents.Quest.Guide.2001032",
        "text": "(<textColor=[215:94:94:255]>삼색조</>의 기름을 \n충분히 모았다. \n이 정도면 넉넉할 거야)"
    },
    "42001033": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 산양유를 이용한 약",
        "id": 42001033,
        "alias": "Contents.Quest.Guide.2001033",
        "text": "(흐르는 폭포 덕분에\n물이 빠르게 정화되겠어)"
    },
    "42001034": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 좋은 붓 제작",
        "id": 42001034,
        "alias": "Contents.Quest.Guide.2001034",
        "text": "(충분히 구했다. \n이 정도면 붓을 만들기 \n충분할 거야)"
    },
    "42001035": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 묘면족 방어",
        "id": 42001035,
        "alias": "Contents.Quest.Guide.2001035",
        "text": "(휴~ 힘들었다. 이 지역은 \n<textColor=[215:94:94:255]>묘면족</>과 <textColor=[215:94:94:255]>낙원족</> 때문에 \n조용할 틈이 없구나)"
    },
    "42001036": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 관광객 보호 임무",
        "id": 42001036,
        "alias": "Contents.Quest.Guide.2001036",
        "text": "(모두 처치했다. \n<textColor=[110:216:175:255]>태록림</>은 제법 안전해져 \n관광객이 많나 보군)"
    },
    "42001037": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 작업자 보호 임무",
        "id": 42001037,
        "alias": "Contents.Quest.Guide.2001037",
        "text": "(휴, 끝마쳤다. \n한동안은 안전하게 \n작업할 수 있을 거야)"
    },
    "42001038": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 단단한 가죽 얻기",
        "id": 42001038,
        "alias": "Contents.Quest.Guide.2001038",
        "text": "(모두 구했다. \n<textColor=[255:204:102:255]>악어가죽</>은 항상 \n인기가 많아 보인다)"
    },
    "42001039": {
        "deprecated": false,
        "Dev_Desc": "[LV.49] 해변의 어둑시니",
        "id": 42001039,
        "alias": "Contents.Quest.Guide.2001039",
        "text": "(도대체 어디서 이렇게 \n끝도 없이 나타나는 걸까?)"
    },
    "42001040": {
        "deprecated": false,
        "Dev_Desc": "[LV.49] 별미, 거북알!",
        "id": 42001040,
        "alias": "Contents.Quest.Guide.2001040",
        "text": "(찾았다!\n조심히 다뤄야겠군)"
    },
    "42001041": {
        "deprecated": false,
        "Dev_Desc": "[LV.35] 자연에서 얻는 약재",
        "id": 42001041,
        "alias": "Contents.Quest.Guide.2001041",
        "text": "(흐음... 냄새가 조금\n이상하긴 하지만\n연구할 가치는 있겠어)"
    },
    "42001042": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 에레다 협행 전문가",
        "id": 42001042,
        "alias": "Contents.Quest.Guide.2001042",
        "text": "(<textColor=[78:148:215:255]>에레다</>의 협행을\n충분히 완수했다)"
    },
    "42001043": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 금경철문 협행 전문가",
        "id": 42001043,
        "alias": "Contents.Quest.Guide.2001043",
        "text": "(<textColor=[78:148:215:255]>금경철문</>을 도와\n각종 재료를 수집하는 \n임무를 모두 완료했다)"
    },
    "42001044": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 도화상전 협행 전문가",
        "id": 42001044,
        "alias": "Contents.Quest.Guide.2001044",
        "text": "(<textColor=[78:148:215:255]>도화상전</>의 협행을\n모두 완수했다.\n보상이 기대되는걸?)"
    },
    "42001045": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 수양 협행 전문가",
        "id": 42001045,
        "alias": "Contents.Quest.Guide.2001045",
        "text": "(<textColor=[78:148:215:255]>수양</>이 제보한 \n<textColor=[110:216:175:255]>태록림</> 치안 유지 협행을\n모두 끝마쳤다)"
    },
    "42001046": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 영교원 협행 전문가",
        "id": 42001046,
        "alias": "Contents.Quest.Guide.2001046",
        "text": "(<textColor=[78:148:215:255]>영교원</> 협행을 완수했다. \n이정도면 협행 전문가라 \n불릴 수도 있겠는걸?)"
    },
    "42001047": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 명혜단 협행 전문가",
        "id": 42001047,
        "alias": "Contents.Quest.Guide.2001047",
        "text": "(<textColor=[110:216:175:255]>태록림</>에서 \n<textColor=[78:148:215:255]>명혜단</>이 제보한 협행을 \n모두 완수했다)"
    },
    "42001048": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 비사문 협행 전문가",
        "id": 42001048,
        "alias": "Contents.Quest.Guide.2001048",
        "text": "(<textColor=[78:148:215:255]>태록림의 주민</>들에게\n조금이나마 도움이 되었기를)"
    },
    "42001049": {
        "deprecated": false,
        "Dev_Desc": "[LV.31] 자꾸 없어지는 비법서",
        "id": 42001049,
        "alias": "Contents.Quest.Guide.2001049",
        "text": "(주변의 위협은 \n충분히 제거했어.\n<textColor=[255:204:102:255]>비법서</>를 찾아보자) "
    },
    "42001050": {
        "deprecated": false,
        "Dev_Desc": "[LV.34] 거북 알 채집",
        "id": 42001050,
        "alias": "Contents.Quest.Guide.2001050",
        "text": "(이 정도면 <textColor=[255:204:102:255]>거북알</>을 \n채집할 시간은 \n충분히 벌었어)"
    },
    "42001051": {
        "deprecated": false,
        "Dev_Desc": "[LV.40] 단단한 식물의 줄기",
        "id": 42001051,
        "alias": "Contents.Quest.Guide.2001051",
        "text": "(됐다! 이제 단단한 \n<textColor=[255:204:102:255]>식물 줄기</>를 채집하자)"
    },
    "42001052": {
        "deprecated": false,
        "Dev_Desc": "[LV.40] 잃어버린 짐을 찾아요",
        "id": 42001052,
        "alias": "Contents.Quest.Guide.2001052",
        "text": "(휴, 모두 처치했다. \n이 틈에 <textColor=[255:204:102:255]>봇짐</>을 회수하자!)"
    },
    "42001053": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 언덕길 관리",
        "id": 42001053,
        "alias": "Contents.Quest.Guide.2001053",
        "text": "(이정도면 충분해.\n이제 <textColor=[255:204:102:255]>쓰레기</>를 치워볼까?)"
    },
    "42001054": {
        "deprecated": false,
        "Dev_Desc": "[LV.49] 별미, 거북알!",
        "id": 42001054,
        "alias": "Contents.Quest.Guide.2001054",
        "text": "(<textColor=[255:204:102:255]>거북알</> 주변의 \n위협을 모두 제거했다)"
    },
    "42002000": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 야영지 터 정리",
        "id": 42002000,
        "alias": "Contents.Quest.Guide.2002000",
        "text": "(이 정도면 <textColor=[215:94:94:255]>홍돈족</>을 \n충분히 몰아낸 것 같군)"
    },
    "42002001": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 만다라화의 소음",
        "id": 42002001,
        "alias": "Contents.Quest.Guide.2002001",
        "text": "(휴~ 힘들었다. \n귀가 먹먹할 지경이야)"
    },
    "42002002": {
        "deprecated": false,
        "Dev_Desc": "[LV.53] 훈련생 보호",
        "id": 42002002,
        "alias": "Contents.Quest.Guide.2002002",
        "text": "(임무를 끝마쳤다. \n훈련생들이 다치지 않고 \n훈련할 수 있을 거야)"
    },
    "42002003": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 전갈의 독",
        "id": 42002003,
        "alias": "Contents.Quest.Guide.2002003",
        "text": "(후, 충분히 구했다.\n<textColor=[255:204:102:255]>독</>을 해독하기 위해\n<textColor=[255:204:102:255]>독</>이 필요한 상황이라니)"
    },
    "42002004": {
        "deprecated": false,
        "Dev_Desc": "[LV.53] 벌판 쓰레기 수거",
        "id": 42002004,
        "alias": "Contents.Quest.Guide.2002004",
        "text": "(<textColor=[110:216:175:255]>수수께끼 벌판</>이 \n한결 깨끗해진 것 같군)"
    },
    "42002005": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 좋은 붓 만들기",
        "id": 42002005,
        "alias": "Contents.Quest.Guide.2002005",
        "text": "(<textColor=[255:204:102:255]>여우털</>을 모두 모았다. \n이 정도면 <textColor=[78:148:215:255]>명혜단</>이 \n붓을 만들기에 충분할 거야)"
    },
    "42002006": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 지역주민 보호",
        "id": 42002006,
        "alias": "Contents.Quest.Guide.2002006",
        "text": "(<textColor=[110:216:175:255]>숲의 장막</>의 <textColor=[215:94:94:255]>어둑시니</>\n퇴치 임무를 완수했다)"
    },
    "42002007": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 날 보호해라냥!",
        "id": 42002007,
        "alias": "Contents.Quest.Guide.2002007",
        "text": "(<textColor=[78:148:215:255]>개척바롱단</>이 부탁한\n<textColor=[215:94:94:255]>검은새벽단</> 퇴치 \n임무를 끝마쳤다)"
    },
    "42002008": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 또 새로운 염료 개발",
        "id": 42002008,
        "alias": "Contents.Quest.Guide.2002008",
        "text": "(이게 바로 혁명적인 \n검은색이란 말이지? \n이 정도면 충분하겠어)"
    },
    "42002009": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 호랑이 가죽 구해요",
        "id": 42002009,
        "alias": "Contents.Quest.Guide.2002009",
        "text": "(부귀를 가져오고 \n잡귀를 몰아낸다라... \n나도 하나 챙겨야 하나?)"
    },
    "42002010": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 영원한 안식",
        "id": 42002010,
        "alias": "Contents.Quest.Guide.2002010",
        "text": "(...편안하게 잠들길)"
    },
    "42002011": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 균열 연구 자료",
        "id": 42002011,
        "alias": "Contents.Quest.Guide.2002011",
        "text": "(모두 모았다. \n이 정도면 <textColor=[255:204:102:255]>균열의 흔적</>을 \n연구하기에 충분할 거야)"
    },
    "42002012": {
        "deprecated": false,
        "Dev_Desc": "[LV.60] 야금 야금 야금!",
        "id": 42002012,
        "alias": "Contents.Quest.Guide.2002012",
        "text": "(휴, 끝마쳤다.\n숨어서 <textColor=[255:204:102:255]>약초</>를 채집하다니,\n<textColor=[78:148:215:255]>바롱</>은 정말 대단한걸)"
    },
    "42002013": {
        "deprecated": false,
        "Dev_Desc": "[LV.60] 좋은 약초 찾기",
        "id": 42002013,
        "alias": "Contents.Quest.Guide.2002013",
        "text": "(...평범한 들풀 같은데... \n일단은 <textColor=[78:148:215:255]>바롱</>들에게 \n가져다주는 게 좋겠다)"
    },
    "42002014": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 붉은 뿔의 가치",
        "id": 42002014,
        "alias": "Contents.Quest.Guide.2002014",
        "text": "(이 정도면 되겠지? \n장식품으로 사용하기에 \n적합해 보인다)"
    },
    "42002015": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 하늘의 해파리",
        "id": 42002015,
        "alias": "Contents.Quest.Guide.2002015",
        "text": "(제법 많이 모았다.\n생, 건조, 반건조, 냉동까지\n시험할 수 있을 양이다)"
    },
    "42002016": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 투연초 몰아내기",
        "id": 42002016,
        "alias": "Contents.Quest.Guide.2002016",
        "text": "(제법 많이 처치했지만\n병사들이 쉬기에는\n아직도 위험해 보인다)"
    },
    "42002017": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 신규 염료 개발",
        "id": 42002017,
        "alias": "Contents.Quest.Guide.2002017",
        "text": "(휴, 이 정도면 충분해. \n<textColor=[78:148:215:255]>도화상전</>은 염료 개발에 \n많은 투자를 하는 것 같군)"
    },
    "42002018": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 화염초의 가치",
        "id": 42002018,
        "alias": "Contents.Quest.Guide.2002018",
        "text": "(드디어 끝마쳤다.\n장인이 기대하는 능력이\n있었으면 좋겠군)"
    },
    "42002019": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 유강독모의 껍데기",
        "id": 42002019,
        "alias": "Contents.Quest.Guide.2002019",
        "text": "(모두 모았다. <textColor=[215:94:94:255]>유강독모</>에 \n내성을 가진 물질이 있다면\n큰 도움이 될텐데...)"
    },
    "42002020": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 담인초의 용도",
        "id": 42002020,
        "alias": "Contents.Quest.Guide.2002020",
        "text": "(<textColor=[215:94:94:255]>담인초</> 조직으로 \n무얼 하려는 걸까? \n이 정도면 충분하겠지?)"
    },
    "42002021": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 좋은 품질의 석재",
        "id": 42002021,
        "alias": "Contents.Quest.Guide.2002021",
        "text": "(당분간은 장인들이 \n안전하게 돌을 \n캘 수 있을 거야)"
    },
    "42002022": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 양피지 재료",
        "id": 42002022,
        "alias": "Contents.Quest.Guide.2002022",
        "text": "(좋은 가죽이야. 고급 \n양피지를 만들 수 있겠어)"
    },
    "42002023": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 변이된 자의 도구",
        "id": 42002023,
        "alias": "Contents.Quest.Guide.2002023",
        "text": "(흐음... 그냥 오래되어\n녹슨 도구처럼 보인다)"
    },
    "42002024": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 창도단 퇴치",
        "id": 42002024,
        "alias": "Contents.Quest.Guide.2002024",
        "text": "(<textColor=[110:216:175:255]>비옥 대지</>는 아직 위험한 \n곳이 많아서 <textColor=[78:148:215:255]>바롱</>이 약초를 \n구하기 힘들 것 같아)"
    },
    "42002025": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 홍돈족 쫓아내기",
        "id": 42002025,
        "alias": "Contents.Quest.Guide.2002025",
        "text": "(<textColor=[215:94:94:255]>홍돈족</>을 쫓아냈다.\n하지만 그들이 <textColor=[110:216:175:255]>붉은 숲길</>을\n쉽게 포기할 것 같진 않다)"
    },
    "42002026": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 창도단 장비 수거",
        "id": 42002026,
        "alias": "Contents.Quest.Guide.2002026",
        "text": "(<textColor=[78:148:215:255]>매화단</>의 장비에 비해서 \n투박해 보이지만, \n조사를 해볼 만은 하다)"
    },
    "42002027": {
        "deprecated": false,
        "Dev_Desc": "[LV.57] 청인족 퇴치 임무",
        "id": 42002027,
        "alias": "Contents.Quest.Guide.2002027",
        "text": "(휴, 끝마쳤다. \n당분간은 문제없을 거야)"
    },
    "42002028": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 풍백문 협행 전문가",
        "id": 42002028,
        "alias": "Contents.Quest.Guide.2002028",
        "text": "(<textColor=[78:148:215:255]>풍백문</>의 협행을\n2건 완료했다.\n보상을 수령하자)"
    },
    "42002029": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 금경철문 협행 전문가",
        "id": 42002029,
        "alias": "Contents.Quest.Guide.2002029",
        "text": "(좋은 재료에 대한 \n<textColor=[78:148:215:255]>금경철문</>의 열정은\n식지 않는구나)"
    },
    "42002030": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 도화상전 협행 전문가",
        "id": 42002030,
        "alias": "Contents.Quest.Guide.2002030",
        "text": "(비옥대지에서 <textColor=[78:148:215:255]>도화상전</>의\n협행을 2건 완수했다.\n보상을 수령하자)"
    },
    "42002031": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 수양 협행 전문가",
        "id": 42002031,
        "alias": "Contents.Quest.Guide.2002031",
        "text": "(<textColor=[78:148:215:255]>수양</>이 의뢰한 협행\n2건을 끝마쳤다.\n보상을 수령하자)"
    },
    "42002032": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 영교원 협행 전문가",
        "id": 42002032,
        "alias": "Contents.Quest.Guide.2002032",
        "text": "(<textColor=[78:148:215:255]>영교원</>이 의뢰한\n협행을 2번 수행했다)"
    },
    "42002033": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 명혜단 협행 전문가",
        "id": 42002033,
        "alias": "Contents.Quest.Guide.2002033",
        "text": "(<textColor=[78:148:215:255]>명혜단</>에서 의뢰한\n협행을 2건 완료했다)"
    },
    "42002034": {
        "deprecated": false,
        "Dev_Desc": "[LV. - ] 비사문 협행 전문가",
        "id": 42002034,
        "alias": "Contents.Quest.Guide.2002034",
        "text": "(<textColor=[78:148:215:255]>비옥대지의 주민</>들에게\n조금이나마 도움이 되었기를)"
    },
    "42002035": {
        "deprecated": false,
        "Dev_Desc": "도화상전 상인",
        "id": 42002035,
        "alias": "Contents.Quest.Guide.2002035",
        "text": "도화상전 상인"
    },
    "42002036": {
        "deprecated": false,
        "Dev_Desc": "명혜단 담당자",
        "id": 42002036,
        "alias": "Contents.Quest.Guide.2002036",
        "text": "명혜단 담당자"
    },
    "42002037": {
        "deprecated": false,
        "Dev_Desc": "수양의 관리자",
        "id": 42002037,
        "alias": "Contents.Quest.Guide.2002037",
        "text": "수양의 관리자"
    },
    "42002038": {
        "deprecated": false,
        "Dev_Desc": "에레다 단원",
        "id": 42002038,
        "alias": "Contents.Quest.Guide.2002038",
        "text": "에레다 단원"
    },
    "42002039": {
        "deprecated": false,
        "Dev_Desc": "금경철문 장인",
