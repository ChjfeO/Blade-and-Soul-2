{
    "40035000": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(거미 숲 길)",
        "id": 40035000,
        "alias": "Contents.Quest.Guide.35000",
        "text": "(세상에... 이 길의 이름이\n<textColor=[110:216:175:255]>거미 숲 길</>인 이유를 알겠군)"
    },
    "40035001": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(거미 숲 길)",
        "id": 40035001,
        "alias": "Contents.Quest.Guide.35001",
        "text": "(역시, <textColor=[110:216:175:255]>반달 호수</>와는\n사뭇 다른 분위기인걸?\n다음 탐사 대상을 찾아볼까)"
    },
    "40035002": {
        "deprecated": false,
        "Dev_Desc": "[Lv.31] 탐사(동부 생태 조...)",
        "id": 40035002,
        "alias": "Contents.Quest.Guide.35002",
        "text": "(<textColor=[110:216:175:255]>생태림</>이라고 하기엔...\n설마 저 <textColor=[215:94:94:255]>시귀</> 때문인가?\n자세히 살펴보자!)"
    },
    "40035003": {
        "deprecated": false,
        "Dev_Desc": "[Lv.31] 탐사(동부 생태 조...)",
        "id": 40035003,
        "alias": "Contents.Quest.Guide.35003",
        "text": "(<textColor=[215:94:94:255]>침식된 고대 묘면족</>이라... \n<textColor=[110:216:175:255]>해월대륙</>도 <textColor=[255:204:102:255]>균열</>로부터 \n안전하지 않은가 보군)"
    },
    "40035004": {
        "deprecated": false,
        "Dev_Desc": "[Lv.32] 탐사(녹색 뿔의 숲)",
        "id": 40035004,
        "alias": "Contents.Quest.Guide.35004",
        "text": "(굉장히 위협적인 뿔이로군.\n이번 탐사 대상은\n<textColor=[215:94:94:255]>녹각다귀</>가 좋겠어)"
    },
    "40035005": {
        "deprecated": false,
        "Dev_Desc": "[Lv.32] 탐사(녹색 뿔의 숲)",
        "id": 40035005,
        "alias": "Contents.Quest.Guide.35005",
        "text": "(<textColor=[110:216:175:255]>녹색 뿔의 숲</>은\n충분히 둘러본 것 같아)"
    },
    "40035006": {
        "deprecated": false,
        "Dev_Desc": "[Lv.33] 탐사(만다라 폭포 앞)",
        "id": 40035006,
        "alias": "Contents.Quest.Guide.35006",
        "text": "(여긴 거의 <textColor=[215:94:94:255]>만다라화</> 밭이네.\n그렇다면 이번 탐사는!)"
    },
    "40035007": {
        "deprecated": false,
        "Dev_Desc": "[Lv.33] 탐사(만다라 폭포 앞)",
        "id": 40035007,
        "alias": "Contents.Quest.Guide.35007",
        "text": "(용케도 <textColor=[215:94:94:255]>만다라화</>의 비명을\n견디며 살아가고 있구나...)"
    },
    "40035008": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(버려진 감시탑)",
        "id": 40035008,
        "alias": "Contents.Quest.Guide.35008",
        "text": "(오호, 물이 얕아 충분히 \n건널 수 있겠는데? \n저 반대편 해변도 살펴보자!)"
    },
    "40035009": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(버려진 감시탑)",
        "id": 40035009,
        "alias": "Contents.Quest.Guide.35009",
        "text": "(<textColor=[110:216:175:255]>가는 모래 하구</>와 \n비슷한 분위기의 해변이네)"
    },
    "40035010": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(버려진 감시탑)",
        "id": 40035010,
        "alias": "Contents.Quest.Guide.35010",
        "text": "(<textColor=[110:216:175:255]>버려진 감시탑</>을\n살피기 위해 온건가?)"
    },
    "40035011": {
        "deprecated": false,
        "Dev_Desc": "[Lv.35] 탐사(난파선의 잔해)",
        "id": 40035011,
        "alias": "Contents.Quest.Guide.35011",
        "text": "(처음 보는 녀석들이야. \n탐사 대상으로 적당하겠어)"
    },
    "40035012": {
        "deprecated": false,
        "Dev_Desc": "[Lv.35] 탐사(난파선의 잔해)",
        "id": 40035012,
        "alias": "Contents.Quest.Guide.35012",
        "text": "(후, 정말 소름이 돋을 정도로 \n무시무시한 녀석이었어)"
    },
    "40035013": {
        "deprecated": false,
        "Dev_Desc": "[Lv.37] 탐사(발자국 없는...)",
        "id": 40035013,
        "alias": "Contents.Quest.Guide.35013",
        "text": "(이쪽 해안에는 <textColor=[215:94:94:255]>악어</>와 \n<textColor=[215:94:94:255]>소라게</>가 살고 있구나)"
    },
    "40035014": {
        "deprecated": false,
        "Dev_Desc": "[Lv.37] 탐사(발자국 없는...)",
        "id": 40035014,
        "alias": "Contents.Quest.Guide.35014",
        "text": "(배가 부서지며 <textColor=[215:94:94:255]>창도단</>이 \n갈 곳을 잃은 걸까?)"
    },
    "40035015": {
        "deprecated": false,
        "Dev_Desc": "[Lv.39] 탐사(난파선의 무덤)",
        "id": 40035015,
        "alias": "Contents.Quest.Guide.35015",
        "text": "(음? 이곳에도 <textColor=[215:94:94:255]>시귀</>가!)"
    },
    "40035016": {
        "deprecated": false,
        "Dev_Desc": "[Lv.39] 탐사(난파선의 무덤)",
        "id": 40035016,
        "alias": "Contents.Quest.Guide.35016",
        "text": "(함께 온 <textColor=[215:94:94:255]>병사</>들이었구나.\n모두 평안히 잠들기를...)"
    },
    "40035017": {
        "deprecated": false,
        "Dev_Desc": "[Lv.40] 탐사(골짜기 바람길)",
        "id": 40035017,
        "alias": "Contents.Quest.Guide.35017",
        "text": "(어라? <textColor=[215:94:94:255]>각다귀</>잖아? \n조사를 해보는 게 좋겠어)"
    },
    "40035018": {
        "deprecated": false,
        "Dev_Desc": "[Lv.40] 탐사(골짜기 바람길)",
        "id": 40035018,
        "alias": "Contents.Quest.Guide.35018",
        "text": "(<textColor=[110:216:175:255]>거점</> 바로 앞에 <textColor=[215:94:94:255]>불마자</>라니.\n방어에 신경 써야겠어)"
    },
    "40035019": {
        "deprecated": false,
        "Dev_Desc": "[Lv.41] 탐사(혈비의 숲)",
        "id": 40035019,
        "alias": "Contents.Quest.Guide.35019",
        "text": "(<textColor=[215:94:94:255]>혈비패</>라… \n<textColor=[215:94:94:255]>청인족</> 패거리로군.\n자세히 조사해보자)"
    },
    "40035020": {
        "deprecated": false,
        "Dev_Desc": "[Lv.41] 탐사(혈비의 숲)",
        "id": 40035020,
        "alias": "Contents.Quest.Guide.35020",
        "text": "(숲의 나머지 절반은 \n<textColor=[215:94:94:255]>늑대</>가 차지했네)"
    },
    "40035021": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(안개 그늘 숲)",
        "id": 40035021,
        "alias": "Contents.Quest.Guide.35021",
        "text": "(세상에! <textColor=[215:94:94:255]>달빛바람</>이잖아?\n아무래도 이번 탐사 대상을\n찾은 것 같군)"
    },
    "40035022": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(안개 그늘 숲)",
        "id": 40035022,
        "alias": "Contents.Quest.Guide.35022",
        "text": "(휴, <textColor=[215:94:94:255]>환영</>인지 몰랐다면 \n많이 당황스러웠겠어)"
    },
    "40035023": {
        "deprecated": false,
        "Dev_Desc": "[Lv.43] 탐사(메말라 가는...)",
        "id": 40035023,
        "alias": "Contents.Quest.Guide.35023",
        "text": "(이게 <textColor=[215:94:94:255]>환영 인면충</>의 \n본모습이로군? \n좋아, 탐사를 시작하자!)"
    },
    "40035024": {
        "deprecated": false,
        "Dev_Desc": "[Lv.43] 탐사(메말라 가는...)",
        "id": 40035024,
        "alias": "Contents.Quest.Guide.35024",
        "text": "(휴, <textColor=[110:216:175:255]>메말라가는 늪지</>는 \n충분히 둘러본 것 같네)"
    },
    "40035025": {
        "deprecated": false,
        "Dev_Desc": "[Lv.44] 탐사(뱀버섯 군락)",
        "id": 40035025,
        "alias": "Contents.Quest.Guide.35025",
        "text": "(이 <textColor=[110:216:175:255]>군락</>은 <textColor=[215:94:94:255]>서인족</>이 \n점령해버렸군. \n탐사를 시작해보자)"
    },
    "40035026": {
        "deprecated": false,
        "Dev_Desc": "[Lv.44] 탐사(뱀버섯 군락)",
        "id": 40035026,
        "alias": "Contents.Quest.Guide.35026",
        "text": "(으으! 정말 하나같이\n<textColor=[215:94:94:255]>위험한 식물</>들이로군)"
    },
    "40035027": {
        "deprecated": false,
        "Dev_Desc": "[Lv.45] 탐사(개척자 위령...)",
        "id": 40035027,
        "alias": "Contents.Quest.Guide.35027",
        "text": "(무슨 일로 <textColor=[215:94:94:255]>혈비패</>가 \n<textColor=[110:216:175:255]>묘지</>까지 온 걸까?)"
    },
    "40035028": {
        "deprecated": false,
        "Dev_Desc": "[Lv.45] 탐사(개척자 위령...)",
        "id": 40035028,
        "alias": "Contents.Quest.Guide.35028",
        "text": "(...이곳이 바로 \n<textColor=[78:148:215:255]>초기 개척자</>들이 묻힌\n<textColor=[110:216:175:255]>위령 묘지</>로구나)"
    },
    "40035029": {
        "deprecated": false,
        "Dev_Desc": "[Lv.46] 탐사(참고요 숲)",
        "id": 40035029,
        "alias": "Contents.Quest.Guide.35029",
        "text": "(이들은 <textColor=[215:94:94:255]>수양</>이잖아? \n여기서 뭐 하는 거지?)"
    },
    "40035030": {
        "deprecated": false,
        "Dev_Desc": "[Lv.46] 탐사(참고요 숲)",
        "id": 40035030,
        "alias": "Contents.Quest.Guide.35030",
        "text": "(<textColor=[110:216:175:255]>참고요 숲</> 탐사는 \n이 정도면 충분해)"
    },
    "40035031": {
        "deprecated": false,
        "Dev_Desc": "[Lv.47] 탐사(담인초 습지)",
        "id": 40035031,
        "alias": "Contents.Quest.Guide.35031",
        "text": "(걸어 다니는... 식물?\n이번 탐사 대상은\n<textColor=[215:94:94:255]>담인초</>가 좋겠어)"
    },
    "40035032": {
        "deprecated": false,
        "Dev_Desc": "[Lv.47] 탐사(담인초 습지)",
        "id": 40035032,
        "alias": "Contents.Quest.Guide.35032",
        "text": "(<textColor=[110:216:175:255]>해월 대륙</>에는 고대의\n흔적이 제법 남아있구나)"
    },
    "40035033": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(낙원 구릉)",
        "id": 40035033,
        "alias": "Contents.Quest.Guide.35033",
        "text": "(<textColor=[215:94:94:255]>낙원족</>이로군!\n이번 탐사로 제격이겠어)"
    },
    "40035034": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(낙원 구릉)",
        "id": 40035034,
        "alias": "Contents.Quest.Guide.35034",
        "text": "(개울가에 모여 사는 건 \n구릉 위에 출몰하는 \n<textColor=[215:94:94:255]>낙원 대성</> 때문이겠지?)"
    },
    "40035035": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(도깨비 해변)",
        "id": 40035035,
        "alias": "Contents.Quest.Guide.35035",
        "text": "(오호, 이 해변에는\n제법 귀여운 녀석들이 많군)"
    },
    "40035036": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(도깨비 해변)",
        "id": 40035036,
        "alias": "Contents.Quest.Guide.35036",
        "text": "(<textColor=[215:94:94:255]>어둑시니</>는 <textColor=[110:216:175:255]>반달호수</>의\n<textColor=[215:94:94:255]>도깨비</>와 닮은 것 같아)"
    },
    "40035037": {
        "deprecated": false,
        "Dev_Desc": "[Lv.50] 탐사(검은 진흙지대)",
        "id": 40035037,
        "alias": "Contents.Quest.Guide.35037",
        "text": "(오호, 이곳에도 <textColor=[215:94:94:255]>거북</>이?\n<textColor=[215:94:94:255]>해변 거북</>과는 또 다르군)"
    },
    "40035038": {
        "deprecated": false,
        "Dev_Desc": "[Lv.50] 탐사(검은 진흙지대)",
        "id": 40035038,
        "alias": "Contents.Quest.Guide.35038",
        "text": "(이 <textColor=[110:216:175:255]>진흙 지대</>에는\n위협적인 생물이 많군)"
    },
    "40035039": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(해맞이 비탈길)",
        "id": 40035039,
        "alias": "Contents.Quest.Guide.35039",
        "text": "(<textColor=[110:216:175:255]>해맞이 비탈길</>은 \n<textColor=[215:94:94:255]>묘면족</>의 영역인가 보군. \n한번 둘러보자)"
    },
    "40035040": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(해맞이 비탈길)",
        "id": 40035040,
        "alias": "Contents.Quest.Guide.35040",
        "text": "(<textColor=[215:94:94:255]>묘면족</> 모두 인간을\n경계하며 적대하고 있어...)"
    },
    "40035041": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(풀내음 언덕길)",
        "id": 40035041,
        "alias": "Contents.Quest.Guide.35041",
        "text": "(윽... <textColor=[215:94:94:255]>시귀</> 때문에 \n풀내음은커녕 썩은내가 \n진동을 하는 군)"
    },
    "40035042": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(풀내음 언덕길)",
        "id": 40035042,
        "alias": "Contents.Quest.Guide.35042",
        "text": "(언덕길 생물들이\n<textColor=[215:94:94:255]>시귀</>를 피해 모여 있구나)"
    },
    "40035043": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(폭포 남부 숲)",
        "id": 40035043,
        "alias": "Contents.Quest.Guide.35043",
        "text": "(깜짝이야! \n정말 거대한 <textColor=[215:94:94:255]>사묘정</>이네!)"
    },
    "40035044": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(폭포 남부 숲)",
        "id": 40035044,
        "alias": "Contents.Quest.Guide.35044",
        "text": "(물가에 저건 <textColor=[215:94:94:255]>야금</>이잖아?\n어딘가 조금 달라 보여)"
    },
    "40035045": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(폭포 남부 숲)",
        "id": 40035045,
        "alias": "Contents.Quest.Guide.35045",
        "text": "(강하고 난폭하네. \n이 정도면 <textColor=[110:216:175:255]>폭포 남부 숲</>은 \n충분히 둘러본 것 같아)"
    },
    "40035046": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(물안개 고원)",
        "id": 40035046,
        "alias": "Contents.Quest.Guide.35046",
        "text": "(짙은 물안개가\n신비로운 <textColor=[110:216:175:255]>고원</>이네.\n주변을 살펴보자)"
    },
    "40035047": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(물안개 고원)",
        "id": 40035047,
        "alias": "Contents.Quest.Guide.35047",
        "text": "(도대체 무슨 목적으로\n<textColor=[110:216:175:255]>고원</>까지 온 걸까?)"
    },
    "40035048": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(물안개 고원)",
        "id": 40035048,
        "alias": "Contents.Quest.Guide.35099",
        "text": "(이 정도면 <textColor=[110:216:175:255]>태록림</>도\n얼추 다 돌아본 것 같아)"
    },
    "40035049": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(그늘 숲 북부)",
        "id": 40035049,
        "alias": "Contents.Quest.Guide.35048",
        "text": "(<textColor=[110:216:175:255]>안개 그늘 숲 북부</>도\n<textColor=[215:94:94:255]>환영</>으로 가득하구나)"
    },
    "40035050": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(그늘 숲 북부)",
        "id": 40035050,
        "alias": "Contents.Quest.Guide.35049",
        "text": "(어둡고 축축한 늪에\n짙은 안개까지…\n음습한 숲이었어)"
    },
    "40035051": {
        "deprecated": false,
        "Dev_Desc": "[Lv.32] 탐사(언덕 숲)",
        "id": 40035051,
        "alias": "Contents.Quest.Guide.35050",
        "text": "(오호, 확실히 <textColor=[110:216:175:255]>수월 평원</>의\n<textColor=[215:94:94:255]>늑대</>와는 다른 모습이네.\n자세히 살펴보자!)"
    },
    "40035052": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(가는 모래 하구)",
        "id": 40035052,
        "alias": "Contents.Quest.Guide.35051",
        "text": "(우와, 여기 이렇게\n멋진 해변이 있었네.\n탐사를 시작해볼까?)"
    },
    "40035053": {
        "deprecated": false,
        "Dev_Desc": "[Lv.31] 탐사(동부 생태 조...)",
        "id": 40035053,
        "alias": "Contents.Quest.Guide.35102",
        "text": "(살아있는 존재를 \n무차별적으로 공격하는군. \n언덕 위로도 올라가 보자)"
    },
    "40035054": {
        "deprecated": false,
        "Dev_Desc": "[Lv.32] 탐사(녹색 뿔의 숲)",
        "id": 40035054,
        "alias": "Contents.Quest.Guide.35104",
        "text": "(<textColor=[215:94:94:255]>주변 생물</>들도\n좀 더 살펴볼까?)"
    },
    "40035055": {
        "deprecated": false,
        "Dev_Desc": "[Lv.33] 탐사(만다라 폭포 앞)",
        "id": 40035055,
        "alias": "Contents.Quest.Guide.35106",
        "text": "(조금 떨어져서 보면\n바람에 흔들리는 부들 같아)"
    },
    "40035056": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(버려진 감시탑)",
        "id": 40035056,
        "alias": "Contents.Quest.Guide.35109",
        "text": "(으음?! 저 <textColor=[215:94:94:255]>도적</>들은 뭐지?)"
    },
    "40035057": {
        "deprecated": false,
        "Dev_Desc": "[Lv.35] 탐사(난파선의 잔해)",
        "id": 40035057,
        "alias": "Contents.Quest.Guide.35111",
        "text": "(<textColor=[215:94:94:255]>어둑시니</>는 장난기가\n넘치는구나. 어라? 저쪽에!)"
    },
    "40035058": {
        "deprecated": false,
        "Dev_Desc": "[Lv.37] 탐사(발자국 없는...)",
        "id": 40035058,
        "alias": "Contents.Quest.Guide.35113",
        "text": "(잠깐! 저건 <textColor=[215:94:94:255]>창도단</>이잖아?)"
    },
    "40035059": {
        "deprecated": false,
        "Dev_Desc": "[Lv.39] 탐사(난파선의 무덤)",
        "id": 40035059,
        "alias": "Contents.Quest.Guide.35115",
        "text": "(...난파된 배의 선원이었군.\n그렇다면 저 <textColor=[215:94:94:255]>시귀</>들은...)"
    },
    "40035060": {
        "deprecated": false,
        "Dev_Desc": "[Lv.40] 탐사(골짜기 바람길)",
        "id": 40035060,
        "alias": "Contents.Quest.Guide.35117",
        "text": "(휴, 맷집 좋은 녀석들이군.\n세상에! 저기 <textColor=[215:94:94:255]>불마자</>잖아?!)"
    },
    "40035061": {
        "deprecated": false,
        "Dev_Desc": "[Lv.41] 탐사(혈비의 숲)",
        "id": 40035061,
        "alias": "Contents.Quest.Guide.35119",
        "text": "(<textColor=[215:94:94:255]>혈비패</>가 이 숲을 \n본거지로 삼았나 보군)"
    },
    "40035062": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(안개 그늘 숲)",
        "id": 40035062,
        "alias": "Contents.Quest.Guide.35121",
        "text": "(<textColor=[215:94:94:255]>홍매화</>와 <textColor=[215:94:94:255]>곽호룡</>까지? \n<textColor=[215:94:94:255]>주변 생물</>들과 함께\n조사해보자)"
    },
    "40035063": {
        "deprecated": false,
        "Dev_Desc": "[Lv.43] 탐사(메말라 가는...)",
        "id": 40035063,
        "alias": "Contents.Quest.Guide.35123",
        "text": "(...<textColor=[215:94:94:255]>환영 인면충</>이 날 \n모방하진 않았으면 좋겠어.\n주변을 좀 더 둘러볼까?)"
    },
    "40035064": {
        "deprecated": false,
        "Dev_Desc": "[Lv.44] 탐사(뱀버섯 군락)",
        "id": 40035064,
        "alias": "Contents.Quest.Guide.35125",
        "text": "(저 괴상한 것들은...\n설마 식물인 건가?)"
    },
    "40035065": {
        "deprecated": false,
        "Dev_Desc": "[Lv.45] 탐사(개척자 위령...)",
        "id": 40035065,
        "alias": "Contents.Quest.Guide.35127",
        "text": "(설마 <textColor=[110:216:175:255]>위령 묘지</>를 \n도굴하려는 건 아니겠지...?\n안쪽으로 들어가 보자)"
    },
    "40035066": {
        "deprecated": false,
        "Dev_Desc": "[Lv.46] 탐사(참고요 숲)",
        "id": 40035066,
        "alias": "Contents.Quest.Guide.35129",
        "text": "(임무는 내팽개치고\n사리사욕을 채우고 있다니...)"
    },
    "40035067": {
        "deprecated": false,
        "Dev_Desc": "[Lv.47] 탐사(담인초 습지)",
        "id": 40035067,
        "alias": "Contents.Quest.Guide.35131",
        "text": "(휴, 힘들었다.\n간간히 <textColor=[215:94:94:255]>묘면족</>도 보이네)"
    },
    "40035068": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(낙원 구릉)",
        "id": 40035068,
        "alias": "Contents.Quest.Guide.35133",
        "text": "(구릉 옆 개울을 따라 \n다양한 생물이 살고 있네. \n한번 둘러볼까?)"
    },
    "40035069": {
        "deprecated": false,
        "Dev_Desc": "[Lv.49] 탐사(도깨비 해변)",
        "id": 40035069,
        "alias": "Contents.Quest.Guide.35135",
        "text": "(귀여운 외모에 \n방심했다가는 큰일 나겠어)"
    },
    "40035070": {
        "deprecated": false,
        "Dev_Desc": "[Lv.50] 탐사(검은 진흙지대)",
        "id": 40035070,
        "alias": "Contents.Quest.Guide.35137",
        "text": "(길이 복잡하고 어두워서\n헤매는 사람이 많겠는걸...)"
    },
    "40035071": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(해맞이 비탈길)",
        "id": 40035071,
        "alias": "Contents.Quest.Guide.35139",
        "text": "(오호! 저쪽에 활과 \n방패를 든 <textColor=[215:94:94:255]>묘면족</>도 있네!)"
    },
    "40035072": {
        "deprecated": false,
        "Dev_Desc": "[Lv.51] 탐사(풀내음 언덕길)",
        "id": 40035072,
        "alias": "Contents.Quest.Guide.35141",
        "text": "(조금 나아진 것 같네.\n저 안쪽도 살펴보자)"
    },
    "40035073": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(폭포 남부 숲)",
        "id": 40035073,
        "alias": "Contents.Quest.Guide.35143",
        "text": "(이 숲에도 <textColor=[215:94:94:255]>시귀</>가…\n개척에 큰 희생이 따랐다는\n사실은 알고 있었지만…)"
    },
    "40035074": {
        "deprecated": false,
        "Dev_Desc": "[Lv.52] 탐사(물안개 고원)",
        "id": 40035074,
        "alias": "Contents.Quest.Guide.35146",
        "text": "(음?! 설마 저들은 \n<textColor=[215:94:94:255]>검은 새벽단</>?\n자세히 살펴보자!)"
    },
    "40035075": {
        "deprecated": false,
        "Dev_Desc": "[Lv.42] 탐사(그늘 숲 북부)",
        "id": 40035075,
        "alias": "Contents.Quest.Guide.35148",
        "text": "(저 <textColor=[215:94:94:255]>악어</>는 정말 거대하네.\n조심해서 살펴보자!)"
    },
    "40035076": {
        "deprecated": false,
        "Dev_Desc": "[Lv.32] 탐사(언덕 숲)",
        "id": 40035076,
        "alias": "Contents.Quest.Guide.35150",
        "text": "(<textColor=[110:216:175:255]>언덕</> 아래로 내려가\n다음 탐사 대상을 찾아볼까?)"
    },
    "40035077": {
        "deprecated": false,
        "Dev_Desc": "[Lv.34] 탐사(가는 모래 하구)",
        "id": 40035077,
        "alias": "Contents.Quest.Guide.35151",
        "text": "(저 녀석들은 굉장히\n단단해 보이는걸?\n자세히 살펴보자)"
    }
}