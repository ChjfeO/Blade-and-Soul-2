{
    "400015000": {
        "deprecated": false,
        "Dev_Desc": "[Lv.6] 탐사(썩은 벌목지)",
        "id": 400015000,
        "alias": "Contents.Quest.Guide.15000",
        "text": "(잠깐, 저기 저 녀석들은\n새총을 들고 있는 것 같은데?)"
    },
    "400015001": {
        "deprecated": false,
        "Dev_Desc": "[Lv.6] 탐사(썩은 벌목지)",
        "id": 400015001,
        "alias": "Contents.Quest.Guide.15001",
        "text": "(이 <textColor=[110:216:175:255]>벌목지</>는 <textColor=[215:94:94:255]>서인족</>이 \n완전히 차지해버렸군)"
    },
    "400015002": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(얕은 호수)",
        "id": 400015002,
        "alias": "Contents.Quest.Guide.15002",
        "text": "(좋아! 이번 탐사대상은\n<textColor=[110:216:175:255]>호숫가</>의 <textColor=[215:94:94:255]>도둑도치</>다!)"
    },
    "400015003": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(얕은 호수)",
        "id": 400015003,
        "alias": "Contents.Quest.Guide.15003",
        "text": "(껍질이 정말 단단하네.\n저쪽 호수로도 가볼까?)"
    },
    "400015004": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(얕은 호수)",
        "id": 400015004,
        "alias": "Contents.Quest.Guide.15004",
        "text": "(아이쿠, 깜짝이야! \n느닷없이 튀어나오는 건 \n도저히 적응이 안된다니까)"
    },
    "400015005": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(거북호)",
        "id": 400015005,
        "alias": "Contents.Quest.Guide.15005",
        "text": "(오호, 이 호수에는\n<textColor=[215:94:94:255]>거북</>이 정말로 많네!)"
    },
    "400015006": {
        "deprecated": false,
        "Dev_Desc": "[Lv.8] 탐사(드러난 유적지)",
        "id": 400015006,
        "alias": "Contents.Quest.Guide.15006",
        "text": "(이런 곳에 <textColor=[110:216:175:255]>유적지</>가...\n이번 탐사 대상은\n<textColor=[215:94:94:255]>폭주 수호병</>이 좋겠어)"
    },
    "400015007": {
        "deprecated": false,
        "Dev_Desc": "[Lv.8] 탐사(드러난 유적지)",
        "id": 400015007,
        "alias": "Contents.Quest.Guide.15007",
        "text": "(<textColor=[110:216:175:255]>회랑</> 안에 있던 <textColor=[215:94:94:255]>수호병</>과 \n같아 보이는데... \n왜 폭주하게 된 걸까?)"
    },
    "400015008": {
        "deprecated": false,
        "Dev_Desc": "[Lv.14] 탐사(투엽단 근거지)",
        "id": 400015008,
        "alias": "Contents.Quest.Guide.15008",
        "text": "(여기가 <textColor=[215:94:94:255]>투엽단</>의 근거지로군.\n주변을 탐사해보자)"
    },
    "400015009": {
        "deprecated": false,
        "Dev_Desc": "[Lv.14] 탐사(투엽단 근거지)",
        "id": 400015009,
        "alias": "Contents.Quest.Guide.15009",
        "text": "(...이렇게 세력이 강했다니.\n무자비한 밀렵을 막기 위한\n대책 강구가 필요하겠어)"
    },
    "400015010": {
        "deprecated": false,
        "Dev_Desc": "[Lv.15] 탐사(고도시 숲길)",
        "id": 400015010,
        "alias": "Contents.Quest.Guide.15010",
        "text": "(세상에! \n<textColor=[215:94:94:255]>호랑이</>가 이렇게도 많다고?\n<textColor=[110:216:175:255]>숲길</>을 탐사해보자)"
    },
    "400015011": {
        "deprecated": false,
        "Dev_Desc": "[Lv.15] 탐사(고도시 숲길)",
        "id": 400015011,
        "alias": "Contents.Quest.Guide.15011",
        "text": "(오호라, <textColor=[215:94:94:255]>수목정령</>은\n자기 구역이 명확하군.\n이제 다른 곳으로 가보자)"
    },
    "400015012": {
        "deprecated": false,
        "Dev_Desc": "[Lv.16] 탐사(창도단 소굴)",
        "id": 400015012,
        "alias": "Contents.Quest.Guide.15012",
        "text": "(<textColor=[215:94:94:255]>창도단</>의 소굴이 \n이곳에 숨겨져 있었구나! \n탐사를 시작해보자)"
    },
    "400015013": {
        "deprecated": false,
        "Dev_Desc": "[Lv.16] 탐사(창도단 소굴)",
        "id": 400015013,
        "alias": "Contents.Quest.Guide.15013",
        "text": "(위협적이군. <textColor=[110:216:175:255]>반달 호수</>의 \n치안에 큰 위협이 되겠어)"
    },
    "400015014": {
        "deprecated": false,
        "Dev_Desc": "[Lv.18] 탐사(깊은 묘지 숲)",
        "id": 400015014,
        "alias": "Contents.Quest.Guide.15014",
        "text": "(묘지 근처라 그런가...\n으스스한 느낌이야.\n<textColor=[215:94:94:255]>거미</>와 <textColor=[215:94:94:255]>늑대</>를 조사해보자)"
    },
    "400015015": {
        "deprecated": false,
        "Dev_Desc": "[Lv.18] 탐사(깊은 묘지 숲)",
        "id": 400015015,
        "alias": "Contents.Quest.Guide.15015",
        "text": "(성불하지 못한 <textColor=[215:94:94:255]>영혼</>들이\n떠돌고 있구나...)"
    },
    "400015016": {
        "deprecated": false,
        "Dev_Desc": "[Lv.18] 탐사(깊은 묘지 숲)",
        "id": 400015016,
        "alias": "Contents.Quest.Guide.15016",
        "text": "(사납고 공격적인걸 보니 \n원한에서 태어난 \n<textColor=[215:94:94:255]>도깨비</>들인가 보다...)"
    },
    "400015017": {
        "deprecated": false,
        "Dev_Desc": "[Lv.21] 탐사(해지는 목초지)",
        "id": 400015017,
        "alias": "Contents.Quest.Guide.15017",
        "text": "(오호, <textColor=[215:94:94:255]>발발이</>로군! \n좋아, 이번 탐사 대상은\n<textColor=[110:216:175:255]>목초지</>의 <textColor=[215:94:94:255]>발발이</>들이다)"
    },
    "400015018": {
        "deprecated": false,
        "Dev_Desc": "[Lv.21] 탐사(해지는 목초지)",
        "id": 400015018,
        "alias": "Contents.Quest.Guide.15018",
        "text": "(<textColor=[215:94:94:255]>마른 사묘정</>은\n그늘을 벗어나지 않는군)"
    },
    "400015019": {
        "deprecated": false,
        "Dev_Desc": "[Lv.21] 탐사(해지는 목초지)",
        "id": 400015019,
        "alias": "Contents.Quest.Guide.15019",
        "text": "(으음? <textColor=[215:94:94:255]>투엽단</>이잖아? \n여기서 뭘 하고 있는 거지?)"
    },
    "400015020": {
        "deprecated": false,
        "Dev_Desc": "[Lv.23] 탐사(돌 언덕 구릉지)",
        "id": 400015020,
        "alias": "Contents.Quest.Guide.15020",
        "text": "(우와, 정말 거대한 <textColor=[215:94:94:255]>곰</>이다.\n좋았어, 이번 탐사 대상은\n구릉지의 <textColor=[215:94:94:255]>곰</>이다!)"
    },
    "400015021": {
        "deprecated": false,
        "Dev_Desc": "[Lv.23] 탐사(돌 언덕 구릉지)",
        "id": 400015021,
        "alias": "Contents.Quest.Guide.15021",
        "text": "(역시 <textColor=[215:94:94:255]>호랑이</>야.\n힘도 <textColor=[215:94:94:255]>곰</>에 뒤지지 않겠어)"
    },
    "400015022": {
        "deprecated": false,
        "Dev_Desc": "[Lv.23] 탐사(돌 언덕 구릉지)",
        "id": 400015022,
        "alias": "Contents.Quest.Guide.15022",
        "text": "(흠. <textColor=[215:94:94:255]>투엽단</>이 이곳까지\n밀렵의 손길을 뻗쳤군)"
    },
    "400015023": {
        "deprecated": false,
        "Dev_Desc": "[Lv.26] 탐사(파괴된 수와족...)",
        "id": 400015023,
        "alias": "Contents.Quest.Guide.15023",
        "text": "(<textColor=[215:94:94:255]>서인족</>이잖아? \n어딘가 좀 이상한데?\n자세히 살펴보자)"
    },
    "400015024": {
        "deprecated": false,
        "Dev_Desc": "[Lv.26] 탐사(파괴된 수와족...)",
        "id": 400015024,
        "alias": "Contents.Quest.Guide.15024",
        "text": "(모두 자아를 잃었군.\n게다가 이전보다 \n훨씬 강해졌어...)"
    },
    "400015025": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(공허의 구덩이)",
        "id": 400015025,
        "alias": "Contents.Quest.Guide.15025",
        "text": "(...온통 <textColor=[215:94:94:255]>불마자</> 천지로군.\n<textColor=[215:94:94:255]>불마자</>를 조사해보자)"
    },
    "400015026": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(공허의 구덩이)",
        "id": 400015026,
        "alias": "Contents.Quest.Guide.15026",
        "text": "(끝도 없이 쏟아져 나오는군.\n균열 안에는 도대체\n어떤 공간이 있는걸까...)"
    },
    "400015027": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(공허의 구덩이)",
        "id": 400015027,
        "alias": "Contents.Quest.Guide.15027",
        "text": "(이곳을 끝으로 <textColor=[110:216:175:255]>반달호수</>는 \n모두 살펴본 것 같다)"
    },
    "400015028": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(얕은 호수)_도둑도치",
        "id": 400015028,
        "alias": "Contents.Quest.Guide.15102",
        "text": "(<textColor=[215:94:94:255]>도둑도치</> 녀석들, \n이것저것 엄청 훔쳤군!\n오호~저기 <textColor=[215:94:94:255]>소라게</>도 있네?)"
    },
    "400015029": {
        "deprecated": false,
        "Dev_Desc": "[Lv.7] 탐사(거북호)_호수 거북",
        "id": 400015029,
        "alias": "Contents.Quest.Guide.15105",
        "text": "(생각보다 <textColor=[215:94:94:255]>호수거북</>의 \n몸놀림이 굉장히 빠른걸?)"
    },
    "400015030": {
        "deprecated": false,
        "Dev_Desc": "[Lv.8] 탐사(드러난 유적지)",
        "id": 400015030,
        "alias": "Contents.Quest.Guide.15106",
        "text": "(음? 저 <textColor=[215:94:94:255]>폭주 수호병</>은 \n무기가 좀 달라 보여)"
    },
    "400015031": {
        "deprecated": false,
        "Dev_Desc": "[Lv.14] 탐사(투엽단 근거지)",
        "id": 400015031,
        "alias": "Contents.Quest.Guide.15108",
        "text": "(<textColor=[215:94:94:255]>검사, 암살자</>에 <textColor=[215:94:94:255]>궁사</>까지...\n제법 구색을 갖췄군.\n좀 더 살펴볼 필요가 있겠어)"
    },
    "400015032": {
        "deprecated": false,
        "Dev_Desc": "[Lv.15] 탐사(고도시 숲길)",
        "id": 400015032,
        "alias": "Contents.Quest.Guide.15110",
        "text": "(정말 포악한 녀석들이군.\n주민들에게 위협이 되겠어)"
    },
    "400015033": {
        "deprecated": false,
        "Dev_Desc": "[Lv.16] 탐사(창도단 소굴)",
        "id": 400015033,
        "alias": "Contents.Quest.Guide.15112",
        "text": "(제법 훈련된 모습이야.\n<textColor=[215:94:94:255]>궁사</>는 <textColor=[215:94:94:255]>호랑이</>와 함께\n다니는 것 같으니 조심하자)"
    },
    "400015034": {
        "deprecated": false,
        "Dev_Desc": "[Lv.18] 탐사(깊은 묘지 숲)",
        "id": 400015034,
        "alias": "Contents.Quest.Guide.15114",
        "text": "(이렇게 큰 <textColor=[215:94:94:255]>거미</>라니!\n<textColor=[215:94:94:255]>늑대</>보다 더 무서운걸.\n더 안쪽으로 가보자)"
    },
    "400015035": {
        "deprecated": false,
        "Dev_Desc": "[Lv.21] 탐사(해지는 목초지)",
        "id": 400015035,
        "alias": "Contents.Quest.Guide.15117",
        "text": "(...어떻게 보면\n귀여운 것 같기도 하고...\n저 옆쪽 언덕도 살펴보자)"
    },
    "400015036": {
        "deprecated": false,
        "Dev_Desc": "[Lv.23] 탐사(돌 언덕 구릉지)",
        "id": 400015036,
        "alias": "Contents.Quest.Guide.15120",
        "text": "(정말 힘이 세군. \n<textColor=[215:94:94:255]>호랑이</>와 <textColor=[215:94:94:255]>곰</> 중에 \n누가 더 강하려나?)"
    },
    "400015037": {
        "deprecated": false,
        "Dev_Desc": "[Lv.26] 탐사(파괴된 수와족...)",
        "id": 400015037,
        "alias": "Contents.Quest.Guide.15123",
        "text": "(이런! 균열의 기운에 \n침식당한 것 같아!)"
    },
    "400015038": {
        "deprecated": false,
        "Dev_Desc": "[Lv.30] 탐사(공허의 구덩이)",
        "id": 400015038,
        "alias": "Contents.Quest.Guide.15125",
        "text": "(검이 정말 단단하네.\n다른 무기를 든 <textColor=[215:94:94:255]>불마자</>도 \n자세히 살펴보자)"
    }
}