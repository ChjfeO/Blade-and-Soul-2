{
    "40055000": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 탐사 1(붉은 비탈)",
        "id": 40055000,
        "alias": "Contents.Quest.Guide.55000",
        "text": "(<textColor=[215:94:94:255]>홍돈족</>은 어떻게 해월대륙까지 왔을까?)"
    },
    "40055001": {
        "deprecated": false,
        "Dev_Desc": "[LV.50] 탐사 2(붉은 비탈)",
        "id": 40055001,
        "alias": "Contents.Quest.Guide.55001",
        "text": "(<textColor=[215:94:94:255]>만다라화</>의 강아지풀에 속아서는 안돼)"
    },
    "40055002": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 1(붉은 숲길)",
        "id": 40055002,
        "alias": "Contents.Quest.Guide.55002",
        "text": "(맹수들이 위협적이군)"
    },
    "40055003": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 1(붉은 숲길)_2",
        "id": 40055003,
        "alias": "Contents.Quest.Guide.55003",
        "text": "(<textColor=[78:148:215:255]>풍백문</>이 관리를 해 되려 짐승이 많아졌군. 다른 곳은 어떨까?)"
    },
    "40055004": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 2(붉은 숲길)",
        "id": 40055004,
        "alias": "Contents.Quest.Guide.55004",
        "text": "(<textColor=[78:148:215:255]>원정대</>가 토지를 제공했다면 <textColor=[215:94:94:255]>홍돈족</>도 약탈을 일삼지 않았을 거야)"
    },
    "40055005": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 1(원령의 언덕)",
        "id": 40055005,
        "alias": "Contents.Quest.Guide.55005",
        "text": "(이렇게 몰려있다니. <textColor=[215:94:94:255]>검은새벽단</>의 목적을 알아볼 필요가 있겠어)"
    },
    "40055006": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 1(원령의 언덕)_2",
        "id": 40055006,
        "alias": "Contents.Quest.Guide.55006",
        "text": "(이들은 <textColor=[78:148:215:255]>풍백문</>을 견제하고 있군. 본격적인듯 한데...)"
    },
    "40055007": {
        "deprecated": false,
        "Dev_Desc": "[LV.51] 탐사 2(원령의 언덕)",
        "id": 40055007,
        "alias": "Contents.Quest.Guide.55007",
        "text": "(<textColor=[255:204:102:255]>수라</>를 추종한다면 <textColor=[255:204:102:255]>신수</>를 모시는 <textColor=[78:148:215:255]>풍백문</>을 용납할 수 없겠지)"
    },
    "40055008": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 탐사 1(단풍 노을 골...)",
        "id": 40055008,
        "alias": "Contents.Quest.Guide.55008",
        "text": "(<textColor=[215:94:94:255]>전갈</>이 눈에 밟히는군. 조사해봐야겠어)"
    },
    "40055009": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 탐사 1(단풍 노을 골...)_2",
        "id": 40055009,
        "alias": "Contents.Quest.Guide.55009",
        "text": "(<textColor=[215:94:94:255]>전갈</>도 <textColor=[215:94:94:255]>버섯</>도 독을 뿜고 있다니. 위험천만하네)"
    },
    "40055010": {
        "deprecated": false,
        "Dev_Desc": "[LV.52] 탐사 2(단풍 노을 골...)",
        "id": 40055010,
        "alias": "Contents.Quest.Guide.55010",
        "text": "(<textColor=[215:94:94:255]>정령</>들은 사람들로부터 나무를 지키려는 거군)"
    },
    "40055011": {
        "deprecated": false,
        "Dev_Desc": "[LV.53] 탐사 (수수께끼 벌판)",
        "id": 40055011,
        "alias": "Contents.Quest.Guide.55011",
        "text": "(<textColor=[110:216:175:255]>벌판</>에 <textColor=[215:94:94:255]>어둑시니</>가 있군. 왜 사람이 다니지 않지?)"
    },
    "40055012": {
        "deprecated": false,
        "Dev_Desc": "[LV.53] 탐사 (수수께끼 벌판)_2",
        "id": 40055012,
        "alias": "Contents.Quest.Guide.55012",
        "text": "(<textColor=[215:94:94:255]>어둑시니</>가 수수께끼로 장난 치니 <textColor=[110:216:175:255]>수수께끼 벌판</>이라 하게 된 거군)"
    },
    "40055013": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 탐사 1(벌목지)",
        "id": 40055013,
        "alias": "Contents.Quest.Guide.55013",
        "text": "(<textColor=[215:94:94:255]>여우</>때문에 위험한 곳에 <textColor=[110:216:175:255]>벌목지</>라니 이상해. 더 조사해보자)"
    },
    "40055014": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 탐사 1(벌목지)_2",
        "id": 40055014,
        "alias": "Contents.Quest.Guide.55014",
        "text": "(<textColor=[215:94:94:255]>여우</>는 사람이 떠나고 자리를 잡았어. 이곳은 왜 버려졌을까?)"
    },
    "40055015": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 탐사 2(벌목지)",
        "id": 40055015,
        "alias": "Contents.Quest.Guide.55015",
        "text": "(나무를 베고 생긴 <textColor=[215:94:94:255]>만다라화</> 때문에 <textColor=[110:216:175:255]>벌목지</>가 버려진 거구나)"
    },
    "40055016": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 탐사(숲의 장막)",
        "id": 40055016,
        "alias": "Contents.Quest.Guide.55016",
        "text": "(여기 <textColor=[215:94:94:255]>어둑시니</>들은 유달리 잿빛이군. 이유가 뭘까?)"
    },
    "40055017": {
        "deprecated": false,
        "Dev_Desc": "[LV.54] 탐사(숲의 장막)_2",
        "id": 40055017,
        "alias": "Contents.Quest.Guide.55017",
        "text": "(숲의 그늘에서 <textColor=[215:94:94:255]>어둑시니</>가 생겼어. 그럼 숲이 생겨난 이유는?)"
    },
    "40055018": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(잿빛 밀림)",
        "id": 40055018,
        "alias": "Contents.Quest.Guide.55018",
        "text": "(<textColor=[215:94:94:255]>서인족</>은 뭔가에 침식됐어. 죽은 나무만 있는 게 의심스럽군)"
    },
    "40055019": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(잿빛 밀림)_2",
        "id": 40055019,
        "alias": "Contents.Quest.Guide.55019",
        "text": "(생명력을 잃은 숲, 이성을 잃은 <textColor=[215:94:94:255]>서인족</> 모두 <textColor=[255:204:102:255]>탁회암</> 탓이군)"
    },
    "40055020": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 2(잿빛 밀림)",
        "id": 40055020,
        "alias": "Contents.Quest.Guide.55020",
        "text": "(참사가 일어나고 남은 생명은 <textColor=[215:94:94:255]>호랑이</>뿐인가...)"
    },
    "40055021": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(비옥 폐허)",
        "id": 40055021,
        "alias": "Contents.Quest.Guide.55021",
        "text": "(<textColor=[255:204:102:255]>대균열</>도 없는데 <textColor=[215:94:94:255]>불마자</>라니. 여기 몰려있는 이유는 뭐지?)"
    },
    "40055022": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(비옥 폐허)_2",
        "id": 40055022,
        "alias": "Contents.Quest.Guide.55022",
        "text": "(<textColor=[215:94:94:255]>불마자</>는 숲에 막혀서 이동하지 못하는 건가?)"
    },
    "40055023": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 2(비옥 폐허)",
        "id": 40055023,
        "alias": "Contents.Quest.Guide.55023",
        "text": "(설마 <textColor=[110:216:175:255]>비옥대지</> 숲 일부는 <textColor=[215:94:94:255]>불마자</>를 막으려고 생긴걸까?)"
    },
    "40055024": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(비옥 발굴지)",
        "id": 40055024,
        "alias": "Contents.Quest.Guide.55024",
        "text": "(<textColor=[255:204:102:255]>거대 비옥</>이 있는데 <textColor=[78:148:215:255]>수양</>이 손놓고 있는 게 이상해. 조사해보자)"
    },
    "40055025": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(비옥 발굴지)_2",
        "id": 40055025,
        "alias": "Contents.Quest.Guide.55025",
        "text": "(옛 참사를 보고 <textColor=[255:204:102:255]>비옥</>의 무서움을 알았기 때문일까?)"
    },
    "40055026": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 2(비옥 발굴지)",
        "id": 40055026,
        "alias": "Contents.Quest.Guide.55026",
        "text": "(<textColor=[110:216:175:255]>발굴지</> 아래 <textColor=[215:94:94:255]>불마자</> 소굴이 있어서 <textColor=[255:204:102:255]>비옥</>을 포기한 걸까?)"
    },
    "40055027": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(붉은 뿔의 길)",
        "id": 40055027,
        "alias": "Contents.Quest.Guide.55027",
        "text": "(이곳 <textColor=[215:94:94:255]>사슴</>들의 뿔이 눈에 띄는군. 좀 더 조사해볼까?)"
    },
    "40055028": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 1(붉은 뿔의 길)_2",
        "id": 40055028,
        "alias": "Contents.Quest.Guide.55028",
        "text": "(악영향으로 생긴 건 아닌듯 하군. 홀릴 것 같은 붉은 빛이야)"
    },
    "40055029": {
        "deprecated": false,
        "Dev_Desc": "[LV.55] 탐사 2(붉은 뿔의 길)",
        "id": 40055029,
        "alias": "Contents.Quest.Guide.55029",
        "text": "(이곳에도 <textColor=[215:94:94:255]>투엽단</>이?\n설마 뿔을 노리는 건가?)"
    },
    "40055030": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(꿈결의 숲)",
        "id": 40055030,
        "alias": "Contents.Quest.Guide.55030",
        "text": "(<textColor=[215:94:94:255]>해파리</>는 보통 바닷가에 있지 않나? 왜 여기에 있는 거지?)"
    },
    "40055031": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(꿈결의 숲)_2",
        "id": 40055031,
        "alias": "Contents.Quest.Guide.55031",
        "text": "(바라볼 수록 마음이 편안해져... 아차, 정신 차려야지)"
    },
    "40055032": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 2(꿈결의 숲)",
        "id": 40055032,
        "alias": "Contents.Quest.Guide.55032",
        "text": "(<textColor=[215:94:94:255]>삼색조</>의 화려한 깃털은 숲의 환경을 따라 변화한 것 같아)"
    },
    "40055033": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(파석의 갈채)",
        "id": 40055033,
        "alias": "Contents.Quest.Guide.55033",
        "text": "(저 곡괭이는...? <textColor=[255:204:102:255]>비옥</>도 없는 곳에 <textColor=[215:94:94:255]>광부</>들이 왜 있지?)"
    },
    "40055034": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(파석의 갈채)_2",
        "id": 40055034,
        "alias": "Contents.Quest.Guide.55034",
        "text": "(설마 이 많은 <textColor=[215:94:94:255]>광부</>들이 바위산을 뚫고 있었던 걸까?)"
    },
    "40055035": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 2(파석의 갈채)",
        "id": 40055035,
        "alias": "Contents.Quest.Guide.55035",
        "text": "(일대를 휩쓴 변이는 재앙이었겠군. <textColor=[215:94:94:255]>발발이</>마저 변이가...)"
    },
    "40055036": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 3(파석의 갈채)",
        "id": 40055036,
        "alias": "Contents.Quest.Guide.55036",
        "text": "(변이의 여파는 <textColor=[78:148:215:255]>탐사대</>의 관리자들도 피할 수 없었나보군)"
    },
    "40055037": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(솟을바위산)",
        "id": 40055037,
        "alias": "Contents.Quest.Guide.55037",
        "text": "(거대한 벌레들은 조사해 볼 가치가 있겠어)"
    },
    "40055038": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(솟을바위산)_2",
        "id": 40055038,
        "alias": "Contents.Quest.Guide.55038",
        "text": "(이 <textColor=[215:94:94:255]>유충</>들이 모두 자란 모습은 상상도 하고 싶지 않은걸)"
    },
    "40055039": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 2(솟을바위산)",
        "id": 40055039,
        "alias": "Contents.Quest.Guide.55039",
        "text": "(험준한 지형 탓에 <textColor=[215:94:94:255]>담인초</>는 산기슭에 밀집해있군)"
    },
    "40055040": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(사묘정의 숲)",
        "id": 40055040,
        "alias": "Contents.Quest.Guide.55040",
        "text": "(변이의 흔적이 눈에 띄는군. 무엇에 의한 영향이지?)"
    },
    "40055041": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(사묘정의 숲)_2",
        "id": 40055041,
        "alias": "Contents.Quest.Guide.55041",
        "text": "(이질적인 색의 원인은 <textColor=[255:204:102:255]>탁회암</>에서 비롯된 모양이야)"
    },
    "40055042": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 2(사묘정의 숲)",
        "id": 40055042,
        "alias": "Contents.Quest.Guide.55042",
        "text": "(<textColor=[215:94:94:255]>사묘정</>의 변이는 더디게 진행되고 있어. 완전 변이도 시간문제겠지만)"
    },
    "40055043": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(솟을바위 경...)",
        "id": 40055043,
        "alias": "Contents.Quest.Guide.55043",
        "text": "(잠시 쉬어가며 <textColor=[215:94:94:255]>산양</>을 조사하는 것도 좋겠지)"
    },
    "40055044": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 1(솟을바위 경...)_2",
        "id": 40055044,
        "alias": "Contents.Quest.Guide.55044",
        "text": "(거대한 바위 경사로 탓에 <textColor=[215:94:94:255]>산양</>만 살 수 있는 것 같아)"
    },
    "40055045": {
        "deprecated": false,
        "Dev_Desc": "[LV.56] 탐사 2(솟을바위 경...)",
        "id": 40055045,
        "alias": "Contents.Quest.Guide.55045",
        "text": "(병사는 <textColor=[255:204:102:255]>용의 사체</> 진입로 확보를 위해 파견되었던 건가...)"
    },
    "40055046": {
        "deprecated": false,
        "Dev_Desc": "[LV.57] 탐사 1(탁회암 협곡)",
        "id": 40055046,
        "alias": "Contents.Quest.Guide.55046",
        "text": "(<textColor=[255:204:102:255]>거대 탁회암</> 근처의 <textColor=[215:94:94:255]>청인족</>은 더 강할까? 조사해 보자)"
    },
    "40055047": {
        "deprecated": false,
        "Dev_Desc": "[LV.57] 탐사 1(탁회암 협곡)_2",
        "id": 40055047,
        "alias": "Contents.Quest.Guide.55047",
        "text": "(다른 점은 보이지 않는군. <textColor=[255:204:102:255]>탁회암</>이 영향은 주지 않나봐)"
    },
    "40055048": {
        "deprecated": false,
        "Dev_Desc": "[LV.57] 탐사 2(탁회암 협곡)",
        "id": 40055048,
        "alias": "Contents.Quest.Guide.55048",
        "text": "(그나저나 <textColor=[215:94:94:255]>청인족</>이 주변에 참 많이도 모여있군)"
    },
    "40055049": {
        "deprecated": false,
        "Dev_Desc": "[LV.57] 탐사 3(탁회암 협곡)",
        "id": 40055049,
        "alias": "Contents.Quest.Guide.55049",
        "text": "(<textColor=[215:94:94:255]>청인족</>도 <textColor=[255:204:102:255]>탁회암</>에 의해 변화하기 전에는 평범한 이였겠지)"
    },
    "40055050": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 탐사 1(용머리 곶)",
        "id": 40055050,
        "alias": "Contents.Quest.Guide.55050",
        "text": "(<textColor=[215:94:94:255]>창도단</>이 주변을 배회하는 게 신경쓰여. 좀 더 조사해볼까?)"
    },
    "40055051": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 탐사 1(용머리 곶)_2",
        "id": 40055051,
        "alias": "Contents.Quest.Guide.55051",
        "text": "(역시 <textColor=[110:216:175:255]>탐사 기지</>를 노리고 있군)"
    },
    "40055052": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 탐사 2(용머리 곶)",
        "id": 40055052,
        "alias": "Contents.Quest.Guide.55052",
        "text": "(<textColor=[78:148:215:255]>탐사대</>가 만든 <textColor=[110:216:175:255]>기지</>를\n<textColor=[215:94:94:255]>창도단</>이 차지해버렸군)"
    },
    "40055053": {
        "deprecated": false,
        "Dev_Desc": "[LV.58] 탐사 3(용머리 곶)",
        "id": 40055053,
        "alias": "Contents.Quest.Guide.55053",
        "text": "(이렇게 경비가 삼엄해서야, <textColor=[78:148:215:255]>매화단</>도 곤란하겠어)"
    },
