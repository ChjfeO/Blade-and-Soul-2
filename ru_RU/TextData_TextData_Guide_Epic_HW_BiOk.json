    },
    "40040002": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040002,
        "alias": "Contents.Quest.Guide.40002",
        "text": "А теперь пойдем туда \n и подождем."
    },
    "40040003": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040003,
        "alias": "Contents.Quest.Guide.40003",
        "text": "Давай пошли!"
    },
    "40040004": {
        "deprecated": false,
        "Dev_Desc": "홍도니",
        "id": 40040004,
        "alias": "Contents.Quest.Guide.40004",
