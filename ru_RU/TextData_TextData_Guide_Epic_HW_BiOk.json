    "40040002": {
        "deprecated": false,
        "Dev_Desc": "아랑",
        "id": 40040002,
        "alias": "Contents.Quest.Guide.40002",
        "text": "자, 이제 저기 가서 \n기다리자."
    },
    "40040003": {
        "deprecated": false,
