    "40040034": {
        "deprecated": false,
        "Dev_Desc": "PC",
        "id": 40040034,
        "alias": "Contents.Quest.Guide.40034",
        "text": "(<textColor=[215:94:94:255]> Я собираюсь победить Черный Рассвет </> \n!)"
    },
    "40040035": {
