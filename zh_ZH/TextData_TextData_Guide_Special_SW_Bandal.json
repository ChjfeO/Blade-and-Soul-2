    "40005083": {
        "deprecated": false,
        "Dev_Desc": "반달스페셜_5101_7_PC",
        "id": 40005083,
        "alias": "Contents.Quest.Guide.5083",
        "text": "(让我们\n对付阻挠的战士,\n找到<textColor=[255:204:102:255]>真理之剑</>)"
