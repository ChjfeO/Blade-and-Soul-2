    "40005090": {
        "deprecated": false,
        "Dev_Desc": "반달스페셜_5102_7_PC",
        "id": 40005090,
        "alias": "Contents.Quest.Guide.5090",
        "text": "(这里使用智妍给的\n<textColor=[255:204:102:255]>信号弹</>)"
    },
    "40005091": {
        "deprecated": false,
        "Dev_Desc": "반달스페셜_5102_8_PC",
        "id": 40005091,
        "alias": "Contents.Quest.Guide.5091",
        "text": "(等一下<textColor=[78:148:215:255]>沉河妍</>)"
    },
    "40005092": {
        "deprecated": false,
        "Dev_Desc": "반달스페셜_5102_9_PC",
        "id": 40005092,
        "alias": "Contents.Quest.Guide.5092",
        "text": "(<textColor=[78:148:215:255]> 是沉河妍</>!)"
    },
    "40005093": {
        "deprecated": false,
        "Dev_Desc": "반달스페셜_5301_3_PC",
        "id": 40005093,
        "alias": "Contents.Quest.Guide.5093",
        "text": "(现在,似乎没有更多的诹访人落后\n)"
