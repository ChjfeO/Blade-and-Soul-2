    "40020094": {
        "deprecated": false,
        "Dev_Desc": "20063",
        "id": 40020094,
        "alias": "Contents.Quest.Guide.20096",
        "text": "(遗憾的是,让我们回到\n<textColor=[110:216:175:255]>迷雾森林边境</>)"
