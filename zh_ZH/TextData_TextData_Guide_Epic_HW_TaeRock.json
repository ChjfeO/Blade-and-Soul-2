    "40020095": {
        "deprecated": false,
        "Dev_Desc": "20064",
        "id": 40020095,
        "alias": "Contents.Quest.Guide.20097",
        "text": "(你想成为证书吗?让我们回到\n<textColor=[78:148:215:255]> Seojungmun</>)"
    },
    "40020096": {
        "deprecated": false,
        "Dev_Desc": "20054",
        "id": 40020096,
        "alias": "Contents.Quest.Guide.20098",
        "text": "哦,是攻击! \n小心!"
    },
    "40020097": {
        "deprecated": false,
        "id": 40020097,
        "alias": "Contents.Quest.Guide.20099",
