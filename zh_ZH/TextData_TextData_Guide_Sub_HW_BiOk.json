    "40050097": {
        "deprecated": false,
        "Dev_Desc": "비옥서브_50106_3_pc 가이드",
        "id": 40050097,
        "alias": "Contents.Quest.Guide.50097",
        "text": "(吃蘑菇要慎重\n)"
    },
    "40050098": {
        "deprecated": false,
        "Dev_Desc": "50003_3_PC",
        "id": 40050098,
        "alias": "Contents.Quest.Guide.50098",
